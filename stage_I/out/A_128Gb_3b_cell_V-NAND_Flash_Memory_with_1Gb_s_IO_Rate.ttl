@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_132c260a a owl:Axiom ;
    rdfs:comment "hasBlockSize rdfs:domain SSD/EnvironmentalAndOperationalContext/WorkloadProfile; rdfs:range xsd:decimal." .

ssd:axiom_13bff237 a owl:Axiom ;
    rdfs:comment "hasStructure rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure." .

ssd:axiom_6f2c0732 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType." .

ssd:axiom_bf3e514f a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_d359182e a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range SSD/EnvironmentalAndOperationalContext/WorkloadProfile." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/2f0c1d722e534c55ab1d28e53884c65d> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E5_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E4_EC1 ;
    dct:source "4KB size read is shorter and important than 16KB size read in a SSD application." .

<http://example.org/ssd/assertion/314cb888a7804a74937bd9c79cfcb8a2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_128_Gb_3b_cell_V_NAND_Flash_Memory ;
    dct:source "We achieved a fast I/O rate of 1 Gb/s." .

<http://example.org/ssd/assertion/aa43428d4ee2437ab967f0d3e35e6124> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_128_Gb_3b_cell_V_NAND_Flash_Memory ;
    dct:source "These short tPROG of, low energy consumption during program and good endurance of over 7 K P/E cycles make it suitable for both client and data-center SSD applications." .

<http://example.org/ssd/assertion/ab663bda44e546338a6591e94c30f080> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_128_Gb_3b_cell_V_NAND_Flash_Memory ;
    dct:source "We succeed in developing 128 Gb 3b/cell Vertical NAND with 32 stack WL layers for the first time." .

<http://example.org/ssd/assertion/c60add5bbfb345c59ea9f8be9d414db8> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasStructure ;
    ssd:subject ssd:E1_128_Gb_3b_cell_V_NAND_Flash_Memory ;
    dct:source "To overcome these limitations, 3D-stacking technology has been developed." .

<http://example.org/ssd/assertion/ce6b394d7605461aa53e171e9f53c608> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_128_Gb_3b_cell_V_NAND_Flash_Memory ;
    dct:source "Good endurance of over 7 K P/E cycles make it suitable for both client and data-center SSD applications." .

<http://example.org/ssd/assertion/f047dac1236941d182aa8907cfa8a941> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "4" ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E5_WP1 ;
    dct:source "4KB size read is shorter and important than 16KB size read in a SSD application." .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:E4_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile ssd:E5_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E5_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    ssd:hasBlockSize "4" ;
    ssd:unit "KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> rdfs:label "3D" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> .

ssd:E1_128_Gb_3b_cell_V_NAND_Flash_Memory a tax:SSD,
        owl:Thing ;
    rdfs:label "128 Gb 3b/cell V-NAND Flash Memory" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasStructure <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E4_EC1 ;
    rdfs:isDefinedBy tax:SSD .

