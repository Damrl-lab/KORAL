<http://example.org/ssd/assertion/013c390bccc84a28be960aa269db24fb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject tax:SSD ;
    dct:source "The current limit for SLC (Single-Level Cell) NAND flash memory is around 100,000 erase/write cycles." .

<http://example.org/ssd/assertion/2be07e3bfec849e38a6ddb663ee84a1e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E6_Group_Based_Wear_Leveling_Algorithm ;
    dct:source "The goal of the wear-leveling algorithm is to expand the lifetime of flash memory by distributing erase operations evenly across the whole flash memory media." .

<http://example.org/ssd/assertion/7e0ae4dd366441e6995f946e079dcddd> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject tax:SSD ;
    dct:source "The new generation of NAND flash memory architecture, MLC (Multi-Level Cell) NAND, has higher density, but the endurance cycle is degraded by an order of magnitude compared to the SLC NAND architecture." .

ssd:E6_Group_Based_Wear_Leveling_Algorithm a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling>,
        owl:Thing ;
    rdfs:label "Group-Based Wear-Leveling Algorithm" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> .

ssd:improves a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC>,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> rdfs:label "Wear Leveling" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> rdfs:label "SLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> .

