<http://example.org/ssd/assertion/2b0cde87f2f24033a07a880f2281a279> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_176_layer_3D_NAND ;
    ssd:predicate ssd:hasManufacturing ;
    ssd:subject ssd:E1_3D_NAND ;
    dct:source "The areal density has been successfully increased more than ten times, and has exceeded 10 Gb/mm2 with 176 layers." .

<http://example.org/ssd/assertion/5d60ca032ae5451fb01424862bb801fb> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_NAND ;
    dct:source "QLC (quad-level cell, 4 bits per cell) is increasing the presence." .

<http://example.org/ssd/assertion/770fde19ae904f9fb609e6b409832fc5> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E4_PLC ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_NAND ;
    dct:source "Several attempts and partial demonstrations were made for PLC (penta-level cell, 5 bits per cell)." .

<http://example.org/ssd/assertion/7c38d8b85fc545bb85ae6ad827af1348> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E6_CMOS_under_Array__CuA_ ;
    ssd:predicate ssd:usesTechnology ;
    ssd:subject ssd:E1_3D_NAND ;
    dct:source "CMOS under array (CuA) enabled the die size reduction and performance improvements." .

<http://example.org/ssd/assertion/863fab318a9346ad80c21df45d3c7a24> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_NAND ;
    dct:source "TLC (triple-level cell, 3 bits per cell) is now the mainstream in 3D NAND." .

ssd:E4_PLC a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType>,
        owl:Thing ;
    rdfs:label "PLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType> .

ssd:E5_176_layer_3D_NAND a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Manufacturing/LayerCount>,
        owl:Thing ;
    rdfs:label "176-layer 3D NAND" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Manufacturing/LayerCount> .

ssd:E6_CMOS_under_Array__CuA_ a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller>,
        owl:Thing ;
    rdfs:label "CMOS under Array (CuA)" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller> .

ssd:hasDeviceType a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> rdfs:label "QLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E1_3D_NAND a tax:SSD,
        owl:Thing ;
    rdfs:label "3D NAND" ;
    ssd:hasDeviceType ssd:E4_PLC,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC>,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasManufacturing ssd:E5_176_layer_3D_NAND ;
    ssd:usesTechnology ssd:E6_CMOS_under_Array__CuA_ ;
    rdfs:isDefinedBy tax:SSD .

