@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_1c6f7099 a owl:Axiom ;
    rdfs:comment "failurePredictedBy rdfs:domain SSD; rdfs:range DerivedIndicators." .

ssd:axiom_4d855a26 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_5263be12 a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:axiom_694b8a9f a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_6fd4b28d a owl:Axiom ;
    rdfs:comment "monitoredBy rdfs:domain SSD; rdfs:range SMARTAttribute." .

ssd:axiom_f271f75d a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/08008a0ac6424eb4bcbf9e4c52bdae6e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E6_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E5_EC1 ;
    dct:source "In data centers, the deployment of SSDs has enabled the seamless handling of massive amount of data and complex computations." .

<http://example.org/ssd/assertion/48216476ae274c628b2761ff19d7f991> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:predicate ssd:monitoredBy ;
    ssd:subject tax:SSD ;
    dct:source "A widely employed scheme for predicting failure in the storage domain is the utilization of SMART(Self-Monitoring, Analysis and Reporting Technology), which encompasses various attributes that span SSD functionality." .

<http://example.org/ssd/assertion/5ada900fb569442591ba787440f1cfa6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E5_EC1 ;
    dct:source "Proactive strategies for predicting solid state drive(SSD) failures are imperative to ensure uninterrupted services in data centers." .

<http://example.org/ssd/assertion/ceca89db89fe4602929344cb1a33dba4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_TCAN ;
    ssd:predicate ssd:failurePredictedBy ;
    ssd:subject tax:SSD ;
    dct:source "In this study, we introduce a temporal-contextual attention network(TCAN), a pioneering method that integrates long short-term memory(LSTM) and transformer architectures to address the limitations of existing schemes." .

<http://example.org/ssd/assertion/eeebd12298a04ca1926dc3e6d96d82de> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Proactive strategies for predicting solid state drive(SSD) failures are imperative to ensure uninterrupted services in data centers." .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:E6_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E7_TCAN a <http://example.org/ssd/taxonomy/SSD/ModelingTimeAndStateHistory/DerivedIndicators>,
        owl:Thing ;
    rdfs:label "TCAN" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ModelingTimeAndStateHistory/DerivedIndicators> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> rdfs:label "SMARTAttribute" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> .

ssd:E5_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E6_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

tax:SSD rdfs:label "SSD" ;
    ssd:failurePredictedBy ssd:E7_TCAN ;
    ssd:monitoredBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:operatesUnder ssd:E5_EC1 ;
    rdfs:isDefinedBy tax:SSD .

