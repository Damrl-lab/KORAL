ssd:axiom_8af68798 a owl:Axiom ;
    rdfs:comment "hasReadWriteMix rdfs:domain EnvironmentalAndOperationalContext/WorkloadProfile; rdfs:range SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix." .

ssd:axiom_9fc74384 a owl:Axiom ;
    rdfs:comment "hasBlockSize rdfs:domain EnvironmentalAndOperationalContext/WorkloadProfile; rdfs:range SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize." .

ssd:axiom_b4d6a01f a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

