<http://example.org/ssd/assertion/17dc2f883ac14233a3ed22d915ef5ff3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "Experimental results show that compared to original elastic striping, our scheme reduces 30.0%–70.6% (and 23.9%–63.2%) of chunk writes under the RAID-5 (and RAID-6) settings, and also reduces the average response time by 60.9%–79.3% (and 56.8%–80.9%) for RAID-5 (and RAID-6), respectively." .

<http://example.org/ssd/assertion/19b3b5c42b6642778d4d4efafdf62613> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "In particular, even if we only classify data chunks into two types, our scheme WAS(2) shown in Fig.7(a) can still reduce up to 70.6% of chunk writes compared with eSAP under Financial1 workload, and it reduces 30.0%–43.2% of chunk writes for other four workloads." .

<http://example.org/ssd/assertion/68b1f56cc9094ce8a83c7f3e39404969> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "Besides, our scheme also improves the endurance and reliability of SSD RAID compared to original elastic striping." .

<http://example.org/ssd/assertion/89a79f3d43954dee827af46946dff71e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "Besides, our scheme also improves the endurance and reliability of SSD RAID compared to original elastic striping." .

<http://example.org/ssd/assertion/a8717f7bec694786b2d95bc29e9e1d09> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "We set the chunk size as 4 KB, and so the stripe size is 32 KB as we have eight SSDs in the RAID." .

<http://example.org/ssd/assertion/ca43a600827442789a459b991df53a2f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_WorkloadProfile ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "To address this problem, we follow the idea of elastic striping and propose a workload-aware scheme (WAS) to reduce the RAID-level GC cost so as to improve the performance and endurance of SSD RAID." .

<http://example.org/ssd/assertion/d57269702667423d851e9fa24224399a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "However, it necessitates an RAID-level garbage collection (GC) process, which may incur a very high cost due to the mixture of hot and cold data chunks." .

<http://example.org/ssd/assertion/dee5e2a97573412c8c201025e34c82db> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "We implement a prototype system on RAID-5 and RAID-6 arrays composed of commercial SSDs." .

<http://example.org/ssd/assertion/eec2a1de47404b4995150157638c86bf> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_SSD_RAID ;
    dct:source "We implement a prototype system on RAID-5 and RAID-6 arrays composed of commercial SSDs." .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> rdfs:label "Write-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "RAID-level Garbage Collection" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Response Time" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> rdfs:label "Reliability" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

ssd:E4_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasBlockSize <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> rdfs:label "RAID-5",
        "RAID-6" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> .

ssd:E1_SSD_RAID a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD_RAID" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:operatesUnder ssd:E4_WorkloadProfile ;
    ssd:uses <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5>,
        <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    rdfs:isDefinedBy tax:SSD .

