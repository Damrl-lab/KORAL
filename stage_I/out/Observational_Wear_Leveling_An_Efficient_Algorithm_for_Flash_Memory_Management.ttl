ssd:axiom_4d8bc290 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic/MediaManagement/Wear Leveling; rdfs:range SSD/InternalFirmwareAndLogic/MediaManagement/Wear Leveling." .

ssd:axiom_5df72011 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic/MediaManagement/Wear Leveling; rdfs:range SSD/MetricsHealthAndState/Endurance." .

ssd:axiom_5f5b896b a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

