<http://example.org/ssd/assertion/59001d9184954bf7b6680640ee904832> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    dct:source "The most commonly used formula for estimating the write amplification with greedy garbage collection as a function of page size and overprovisioning is that of Bux and Iliadis." .

<http://example.org/ssd/assertion/7c965353544f4307a957f7e971816083> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    dct:source "Separating hot and cold data has been shown to reduce write amplification and, respectively, garbage collection costs and cell wear." .

<http://example.org/ssd/assertion/b85418f1568b4f2193486e3ef6418706> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:EC1_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:SSD1_SSD ;
    dct:source "We use SSDPlayer to illustrate data movement in the uniform case, where it is well-understood." .

<http://example.org/ssd/assertion/c02659756a794cb7bf5f9aaf9edd1e61> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:WP1_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:EC1_EnvironmentalAndOperationalContext ;
    dct:source "We use SSDPlayer to illustrate data movement in the uniform case, where it is well-understood." .

<http://example.org/ssd/assertion/c2bcacc5250d45d5be18fa10408c1d8c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:WP1_WorkloadProfile ;
    dct:source "We use the same SSD and FTL with a Zipf workload." .

<http://example.org/ssd/assertion/c9296c0d00784ebe886e6b62203ac955> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    dct:source "Many FTL optimizations incur additional internal data movement. Examples include wear leveling." .

<http://example.org/ssd/assertion/d2aa9e9d09f246558d40e226613fced0> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:WP1_WorkloadProfile ;
    dct:source "We use SSDPlayer to illustrate data movement in the uniform case, where it is well-understood." .

ssd:SSD1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:operatesUnder ssd:EC1_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

