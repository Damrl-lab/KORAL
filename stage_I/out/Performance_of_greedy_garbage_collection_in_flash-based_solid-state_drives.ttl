<http://example.org/ssd/assertion/39969d2dd5214df59696321475e0fcf2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:degrades ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    dct:source "The write amplification, incurred because of the additional write operations performed by the garbage-collection mechanism is a critical factor that negatively affects the lifetime and endurance of SSDs." .

<http://example.org/ssd/assertion/7491cf050ba0448081462974caef8877> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    ssd:predicate ssd:impacts ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    dct:source "The write amplification, incurred because of the additional write operations performed by the garbage-collection mechanism is a critical factor that negatively affects the lifetime and endurance of SSDs." .

<http://example.org/ssd/assertion/914bcfcc9bc246629d472d8dbc134272> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "We develop two complementary theoretical models of the SSD operation for uniformly-distributed random small user writes." .

<http://example.org/ssd/assertion/a11fc4117efa45d8ae26d6da6b062c85> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "In flash-based solid-state drives (SSD) and log-structured filesystems, new data is written out-of-place, which over time exhausts the available free space." .

<http://example.org/ssd/assertion/c329a0fea8e94c8990de2ee63403ba0e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "Random" ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E3_WP1 ;
    dct:source "We develop two complementary theoretical models of the SSD operation for uniformly-distributed random small user writes." .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    ssd:operatesUnder ssd:E2_EC1 ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "Garbage Collection" ;
    ssd:impacts <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile ssd:E3_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E3_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    ssd:hasAccessPattern "Random" ;
    ssd:unit "AccessPattern" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> rdfs:label "Write Amplification" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> .

