<http://example.org/ssd/assertion/0cc6f48ac00b4859bf65e9ea531bf277> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_Diff_RAID ;
    dct:source "Diff-RAID controls the aging rates of different devices in the array by distributing parity blocks unevenly across them. Random writes cause parity blocks to attract much more write traffic than data blocks." .

<http://example.org/ssd/assertion/1c1b3650e7e140609c09180f2b70ad0e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E6_Intel_X25_M ;
    dct:source "We also evaluate Diff-RAIDâ€™s performance using a software implementation on a 5-device array of 80 GB Intel X25-M SSDs." .

<http://example.org/ssd/assertion/3c1ee72763e849059b88534a2577c03b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_Diff_RAID ;
    dct:source "We show that Diff-RAID provides up to four orders of magnitude more reliability than conventional RAID-5 for specific failure modes." .

<http://example.org/ssd/assertion/c3340e94e3b24480b0d680ecd8fa2f2c> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_Diff_RAID ;
    dct:source "Commodity SSDs can offer thousands of random reads and writes per second, potentially eliminating I/O bottlenecks in high-performance data centers." .

ssd:E6_Intel_X25_M a tax:SSD,
        owl:Thing ;
    rdfs:label "Intel X25-M" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    rdfs:isDefinedBy tax:SSD .

