<http://example.org/ssd/assertion/402535b48c6f46fba8e43f9ce7f45ca5> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E2_LDPC_SSD_1 ;
    dct:source "Soft-decision memory sensing directly causes an increased NAND flash memory read latency, leading to storage system read response time degradation." .

<http://example.org/ssd/assertion/5256029daabe477c82acd8f3f60175bb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_LDPC_SSD_1 ;
    dct:source "We quantitatively evaluate these techniques by carrying out trace-based SSD simulations with runtime characterization of NAND flash memory reliability and LDPC code decoding." .

<http://example.org/ssd/assertion/93ae5d3a786d4effb6a4acc64bb89a01> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E2_LDPC_SSD_1 ;
    dct:source "We carry out extensive characterization of 25nm MLC NAND flash memory reliability and LDPC code decoding." .

<http://example.org/ssd/assertion/c50825ad5c5149648f930e53942d1126> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "Read-Heavy" ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E4_EC1 ;
    dct:source "Once NAND flash memory chips are heavily cycled (10,000 P/E cycling in this study), soft-decision memory sensing and LDPC code decoding will be frequently invoked and the system read response time degradation can be even over 100% for read-intensive workloads." .

ssd:degrades a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

ssd:E2_LDPC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "LDPC_SSD_1" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:operatesUnder ssd:E4_EC1 ;
    rdfs:isDefinedBy tax:SSD .

ssd:E4_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile "Read-Heavy" ;
    ssd:unit "ReadWriteMix" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> rdfs:label "EnvironmentalAndOperationalContext" ;
