<http://example.org/ssd/assertion/16ed542307c3412cb1e140723abe9084> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasFlashTechnology ;
    ssd:subject ssd:E1_3D_NAND_Flash ;
    dct:source "Two types of NAND flash technologies –charge-trap (CT) and floating-gate (FG) are presented in this paper to introduce NAND flash designs in detail." .

<http://example.org/ssd/assertion/2698a5541a744dd080ad1b54fb285b0e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "For FG-based 3D NAND, because the FG layer is completely isolated by the TOX layer, the electrons can be stored for longer time due to this reliable structure with the weak leakage paths, and the retention performance is better than CT-based 3D NAND." .

<http://example.org/ssd/assertion/31b005bca8fd4c54ab5fd6d5d8187e88> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:degrades ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "For CT-based NAND, the electrons are deeply trapped in the nitride storage layer. It is not easy to be de-trapped when erasing." .

<http://example.org/ssd/assertion/c76183893e4840bcab3050035607a8be> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasFlashTechnology ;
    ssd:subject ssd:E1_3D_NAND_Flash ;
    dct:source "Two types of NAND flash technologies –charge-trap (CT) and floating-gate (FG) are presented in this paper to introduce NAND flash designs in detail." .

ssd:degrades a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:E1_3D_NAND_Flash a tax:SSD,
        owl:Thing ;
    rdfs:label "3D NAND Flash" ;
    ssd:hasFlashTechnology <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> rdfs:label "Retention" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> rdfs:label "Charge Trap",
        "Floating Gate" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> .

