@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_0cbd0661 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_1d8b421d a owl:Axiom ;
    rdfs:comment "hasReadWriteMix rdfs:domain DeploymentEnvironment; rdfs:range WorkloadProfile/ReadWriteMix." .

ssd:axiom_3c289df5 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_4de70281 a owl:Axiom ;
    rdfs:comment "degradedBy rdfs:domain SSD; rdfs:range MediaManagement/Garbage Collection." .

ssd:axiom_5ac57785 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range MediaManagement/Wear Leveling." .

ssd:axiom_aabcc5c4 a owl:Axiom ;
    rdfs:comment "hasInterfaceType rdfs:domain SSD; rdfs:range InterfaceType." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/5c65a89ca2ab49eca358a27ad77e599a> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E9_WBC_System ;
    dct:source "The median R/W ratio across the entire population is 4.1:1 and the 95th percentile is 150:1 (4.4× higher than for AFF)." .

<http://example.org/ssd/assertion/6da286d81ec64a8b8fbcf91dc01556c9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "How effective are SSDs in production environments at wear leveling?" .

<http://example.org/ssd/assertion/99935d3b7fdc4e608accda5a1ee4400b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_Enterprise_Storage_System ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Our study is based on a rich collection of telemetry data from a large population of enterprise storage systems in production, comprising almost 2 million SSDs." .

<http://example.org/ssd/assertion/bd03f0f4be5f436c939d2a2461c59bc1> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:degradedBy ;
    ssd:subject tax:SSD ;
    dct:source "Moreover, SSDs require background work, such as garbage collection and wear leveling, which generates write amplification and affects a drive’s performance and lifespan." .

<http://example.org/ssd/assertion/c95c5cd421c1469faba4ed0180a0ded3> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E10_AFF_System ;
    dct:source "The median R/W ratio is 3.6:1 and the 95th percentile is 61:1." .

<http://example.org/ssd/assertion/cc73cd1867b745cc94bc9b3cf7dddf33> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject tax:SSD ;
    dct:source "The last three rows involve SSDs with an NVMe storage interface, whereas all the other rows involve SAS drives." .

<http://example.org/ssd/assertion/cd8e72c3e9024bf9b26c15eafb23c19b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject tax:SSD ;
    dct:source "Our study’s SSD population comprises almost 2 million drives, which span 3 manufacturers, 20 different families, 2 interfaces (i.e., SAS and NVMe), and 4 major flash technologies, i.e., cMLC (consumer-class), eMLC (enterprise-class), 3D-TLC, and 3D-eTLC." .

<http://example.org/ssd/assertion/e387e55178544e65b40969bf4fe63342> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SAS> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject tax:SSD ;
    dct:source "The last three rows involve SSDs with an NVMe storage interface, whereas all the other rows involve SAS drives." .

ssd:E10_AFF_System a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment>,
        owl:Thing ;
    rdfs:label "AFF System" ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment> .

ssd:E9_WBC_System a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment>,
        owl:Thing ;
    rdfs:label "WBC System" ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment> .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance/DWPD> rdfs:label "DWPD" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance/DWPD> .

ssd:E2_Enterprise_Storage_System a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment>,
        owl:Thing ;
    rdfs:label "Enterprise Storage System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment> .

ssd:hasReadWriteMix a owl:DatatypeProperty .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "Write Amplification" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> rdfs:label "Wear Leveling" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "3D-TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> rdfs:label "NVMe" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SAS> rdfs:label "SAS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SAS> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> rdfs:label "Read/Write Ratio" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> .

tax:SSD rdfs:label "SSD" ;
    ssd:degradedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe>,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SAS> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    ssd:operatesUnder ssd:E2_Enterprise_Storage_System ;
    rdfs:isDefinedBy tax:SSD .

