<http://example.org/ssd/assertion/93c72fefa44c4edf9061e987b9f31b2a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_Write_Amplification ;
    ssd:predicate ssd:minimizes ;
    ssd:subject ssd:E4_GREEDY ;
    dct:source "GREEDY minimizes the write amplification for random access patterns without temporal locality." .

<http://example.org/ssd/assertion/e8edf53b360744968045c8fa66c9ddc9> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_Random_Access_Pattern ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E4_GREEDY ;
    dct:source "GREEDY minimizes the write amplification for random access patterns without temporal locality." .

ssd:operatesUnder a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

ssd:E3_Write_Amplification a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        owl:Thing ;
    rdfs:label "Write Amplification" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

ssd:E4_GREEDY a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection>,
        owl:Thing ;
    rdfs:label "GREEDY" ;
    ssd:minimizes ssd:E3_Write_Amplification ;
    ssd:operatesUnder ssd:E5_Random_Access_Pattern ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

ssd:E5_Random_Access_Pattern a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern>,
        owl:Thing ;
    rdfs:label "Random Access Pattern" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "Garbage Collection" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

