<http://example.org/ssd/assertion/4c020cd41cad49b8aaae7d4bf29a9be4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Solid-state drives (SSDs) are rapidly replacing hard disk drives (HDDs) in enterprise data centers." .

<http://example.org/ssd/assertion/86f83a40b6b947a5aa8fed63d7a59742> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:predicate ssd:hasFileSystem ;
    ssd:subject ssd:E1_SSD ;
    dct:source "We implement our scheme, FStream, in Linux ext4 and xfs file systems and perform experiments using a variety of workloads." .

<http://example.org/ssd/assertion/88c4ec17960f4c89bd4db1d29e131e22> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Solid-state drives (SSDs) are rapidly replacing hard disk drives (HDDs) in enterprise data centers." .

<http://example.org/ssd/assertion/90e145eb19fb439686bee72bcf97d4e0> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E5_FStream ;
    dct:source "Experimental results show that FStream enhances the filebench performance by 5%–35% and reduces WAF by 7%–46%." .

<http://example.org/ssd/assertion/9e200166d54148f59c04e3beafb5f6cd> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E5_FStream ;
    dct:source "For a NoSQL database benchmark, performance is improved by up to 38% and WAF is reduced by up to 81%." .

<http://example.org/ssd/assertion/bfdcf3ada1584ec1854401a09859a2e3> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E8_Write_Amplification_Factor__WAF_ ;
    ssd:predicate ssd:reduces ;
    ssd:subject ssd:E5_FStream ;
    dct:source "Experimental results show that FStream enhances the filebench performance by 5%–35% and reduces WAF by 7%–46%." .

<http://example.org/ssd/assertion/ebfc5efad2bf418f97c836a03a876bc2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Our experiments show that FStream robustly achieves a near-optimal WAF (close to 1) across the workloads we examined using a stream-capable NVMe (NVM Express) SSD." .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E8_Write_Amplification_Factor__WAF_ a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance>,
        owl:Thing ;
    rdfs:label "Write Amplification Factor (WAF)" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

ssd:improves a owl:ObjectProperty .

ssd:E1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:hasFileSystem <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E5_FStream a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "FStream" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:reduces ssd:E8_Write_Amplification_Factor__WAF_ ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> rdfs:label "File System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> rdfs:label "NVMe" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

