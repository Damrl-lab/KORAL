@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_0a2ec055 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_3ca62418 a owl:Axiom ;
    rdfs:comment "hasInterfaceType rdfs:domain SSD; rdfs:range InterfaceType." .

ssd:axiom_746c1278 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_a67fa471 a owl:Axiom ;
    rdfs:comment "hasFileSystem rdfs:domain SSD; rdfs:range File System." .

ssd:axiom_b3d1126e a owl:Axiom ;
    rdfs:comment "improves rdfs:domain InternalFirmwareAndLogic; rdfs:range Performance." .

ssd:axiom_dd3d0fc0 a owl:Axiom ;
    rdfs:comment "reduces rdfs:domain InternalFirmwareAndLogic; rdfs:range Endurance." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

