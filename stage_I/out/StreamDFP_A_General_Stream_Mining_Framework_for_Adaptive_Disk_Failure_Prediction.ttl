@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_1196c286 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_446d1cfc a owl:Axiom ;
    rdfs:comment "includes rdfs:domain SMARTAttribute; rdfs:range SMARTAttribute." .

ssd:axiom_c1673870 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_c8ced593 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/0e5d085ee80a4dd294b7fd583cfdc109> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E4_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We explore the prediction of imminent disk failures based on machine learning as a proactive fault tolerance mechanism." .

<http://example.org/ssd/assertion/49e0e3faf18d451996fd2d876bdd4ac6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Our evaluation on production workloads (e.g., SMART logs from Backblaze [1]) justifies the effectiveness of machine learning." .

<http://example.org/ssd/assertion/97a1d90cacd14c4eadd6436f3916ce61> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "MAINTAINING storage reliability is a critical yet challenging requirement for modern cloud-scale data centers." .

<http://example.org/ssd/assertion/b961d86bc40a4704a10ec7c9ca7a527a> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Reported%2520Uncorrectable%2520Errors%2520%2520%255BID%253A%2520187%255D> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    dct:source "We further study the changes of several critical SMART attributes defined by Backblaze, which provide strong indicators for disk failures." .

<http://example.org/ssd/assertion/d8883abf51cf4885a72ddebd6a84b37d> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Command%2520Timeout%2520%2520%255BID%253A%2520188%255D> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    dct:source "We further study the changes of several critical SMART attributes defined by Backblaze, which provide strong indicators for disk failures." .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

tax:SSD rdfs:label "SSD" ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> rdfs:label "SMARTAttribute" ;
    ssd:includes <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Command%2520Timeout%2520%2520%255BID%253A%2520188%255D>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Reported%2520Uncorrectable%2520Errors%2520%2520%255BID%253A%2520187%255D> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Command%2520Timeout%2520%2520%255BID%253A%2520188%255D> rdfs:label "Command Timeout" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Command%2520Timeout%2520%2520%255BID%253A%2520188%255D> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Reported%2520Uncorrectable%2520Errors%2520%2520%255BID%253A%2520187%255D> rdfs:label "Reported Uncorrectable Errors" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Reported%2520Uncorrectable%2520Errors%2520%2520%255BID%253A%2520187%255D> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E4_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

