@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_24ce4bb0 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_4142cdfb a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_baa67e59 a owl:Axiom ;
    rdfs:comment "monitors rdfs:domain SSD; rdfs:range SMARTAttribute." .

ssd:axiom_d20fbe9b a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:axiom_ef5ce671 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/30e3c6f45de64251956baffa911be069> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "25" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "We observe that most attributes have a significant change around 7 days before failures." .

<http://example.org/ssd/assertion/44fbe3e2bab348e986c87717a8255198> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The DNN approach learns the distribution of S.M.A.R.T data which varies with disk models." .

<http://example.org/ssd/assertion/564847b2c2d747c69f265997e9f99542> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_HDDse ;
    dct:source "Our evaluation results on two real-world data centers confirm that the proposed system is effective and outperforms several state-of-the-art approaches." .

<http://example.org/ssd/assertion/58110ec618a14ebebef307e4a6a041f2> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "Hard disks are widely used as primary storage devices in modern data centers, where disk failures constantly happen." .

<http://example.org/ssd/assertion/5de475d177e8438caf64b7f98d19aff2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:predicate ssd:monitors ;
    ssd:subject ssd:E2_HDDse ;
    dct:source "Existing work mostly uses the S.M.A.R.T (Self-Monitoring, Analysis and Reporting Technology) data to build a disk failure detection model." .

<http://example.org/ssd/assertion/7fea0312e251491c93bb3bb48e985f92> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object ssd:E5_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "We use the imbalance degree (IDe) to indicate the dataset imbalance which is defined as the ratio between the majority and minority samples." .

<http://example.org/ssd/assertion/9931929cca384c88ad3cac97b236a989> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The DNN approach learns the distribution of S.M.A.R.T data which varies with disk models." .

<http://example.org/ssd/assertion/b0f90577e8244f70981017285ecf64c2> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The DAD method learns the distance (similarity) between the normal and abnormal disk samples in a certain space which has a commonality and not sensitive to the disk models." .

<http://example.org/ssd/assertion/bd7b1db2d50d4b75867820ae4feb23d7> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The DNN approach learns the distribution of S.M.A.R.T data which varies with disk models." .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:E2_HDDse a tax:SSD,
        owl:Thing ;
    rdfs:label "HDDse" ;
    ssd:monitors <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:operatesUnder ssd:E3_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> rdfs:label "AccessPattern" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> rdfs:label "BlockSize" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> rdfs:label "QueueDepth" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> rdfs:label "ReadWriteMix" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> rdfs:label "SMARTAttribute" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> .

ssd:E3_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasTemperature "25" ;
    ssd:hasWorkloadProfile ssd:E5_WorkloadProfile ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E5_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasAccessPattern <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:hasBlockSize <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:hasQueueDepth <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

