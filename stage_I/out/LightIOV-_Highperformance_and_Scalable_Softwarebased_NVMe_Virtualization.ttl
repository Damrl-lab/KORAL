@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_5739084c a owl:Axiom ;
    rdfs:comment "hasInterfaceType rdfs:domain SSD; rdfs:range PhysicalHardwareAndArchitecture/InterfaceType." .

ssd:axiom_928e3943 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_b6fb1350 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext/WorkloadProfile." .

ssd:axiom_c2c5611d a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/023300aa1b404f7bbdbc5036d45fc721> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "We ran 4k-randread-1-1 on these VMs and measured the average latency and overall throughput of all VMs." .

<http://example.org/ssd/assertion/0fad55df2b4740e4906017788435493a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "NVMe SSDs are widely deployed in data centers such as AWS, Alibaba Cloud, Microsoft Azure, and Google Cloud." .

<http://example.org/ssd/assertion/2195f99d6d8d498299ac7f055349d9d6> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "Results from our experiments show that LightIOV can provide comparable performance to VFIO, with an IOPS of 97.6%-100.2% of VFIO." .

<http://example.org/ssd/assertion/4ab53a156c23456e9d6c700e3a28efb4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "LightIOV achieves 31.4% lower latency than SPDK-Vhost when running 200 VMs." .

<http://example.org/ssd/assertion/58ba7b1db67944d29c6e19e277cd75d7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E6_SPDK_Vhost ;
    ssd:predicate ssd:comparedTo ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "LightIOV achieves 31.4% lower latency than SPDK-Vhost when running 200 VMs." .

<http://example.org/ssd/assertion/7e8cdac7912341d5b42fd0f1ad2c4327> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "Random" ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "We ran 4k-randread-1-1 on these VMs and measured the average latency and overall throughput of all VMs." .

<http://example.org/ssd/assertion/a802e1a3e682439d93b11af138f4c076> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD1> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "We ran 4k-randread-1-1 on these VMs and measured the average latency and overall throughput of all VMs." .

<http://example.org/ssd/assertion/af7a4b4eb7014b00ac440e8a07cdea1b> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "The key idea behind LightIOV is NVMe hardware I/O queues passthrough." .

<http://example.org/ssd/assertion/e5d0c04ab82c4f6a8d840584048f7070> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object ssd:E7_VFIO ;
    ssd:predicate ssd:comparedTo ;
    ssd:subject ssd:E1_LightIOV ;
    dct:source "Results from our experiments show that LightIOV can provide comparable performance to VFIO, with an IOPS of 97.6%-100.2% of VFIO." .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> .

ssd:E6_SPDK_Vhost a tax:SSD,
        owl:Thing ;
    rdfs:label "SPDK-Vhost" ;
    rdfs:isDefinedBy tax:SSD .

ssd:E7_VFIO a tax:SSD,
        owl:Thing ;
    rdfs:label "VFIO" ;
    rdfs:isDefinedBy tax:SSD .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD1> rdfs:label "QD1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD1> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> rdfs:label "NVMe" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

ssd:E1_LightIOV a tax:SSD,
        owl:Thing ;
    rdfs:label "LightIOV" ;
    ssd:comparedTo ssd:E6_SPDK_Vhost,
        ssd:E7_VFIO ;
    ssd:hasBlockSize <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:hasQueueDepth <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD1> ;
    ssd:hasWorkloadProfile "Random" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:operatesUnder <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:unit "AccessPattern" ;
    rdfs:isDefinedBy tax:SSD .

