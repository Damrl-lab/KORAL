<http://example.org/ssd/assertion/1874e60a45104935ad05d9a3d6cde025> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E12_ASLDP ;
    dct:source "ASLDP achieves stable failure detection rates of 80–85% with low false alarm rates." .

<http://example.org/ssd/assertion/20bdfea88b9844e2a15bc05a9df2a402> a ssd:Assertion ;
