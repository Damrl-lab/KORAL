@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_2524f97f a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType." .

ssd:axiom_38f5bde2 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_bf17c3e7 a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/6c579adc31514029bc61bf95e7b14bbb> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_MLC_SSD_1 ;
    dct:source "We quantify the reliability improvement provided by each of these techniques." .

<http://example.org/ssd/assertion/6ef819ebfdc841c4bf5e72f1ecc29229> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "We provide rigorous experimental data from state-of-the-art MLC and TLC NAND flash devices on various types of flash memory errors, to motivate the need for such techniques." .

<http://example.org/ssd/assertion/91ed7549c47a4a9bb68bd3d7a038aa17> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "We quantify the reliability improvement provided by each of these techniques." .

<http://example.org/ssd/assertion/ad7e74f34eab4682b598c0345a4e145d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E2_MLC_SSD_1 ;
    dct:source "We provide rigorous experimental data from state-of-the-art MLC and TLC NAND flash devices on various types of flash memory errors, to motivate the need for such techniques." .

<http://example.org/ssd/assertion/afbafc1a4b5f4be3800a60ce2a8d2f34> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_MLC_SSD_1 ;
    dct:source "SSDs deliver significantly higher read and write performance, with orders of magnitude of improvement in random-access input/output (I/O) operations." .

<http://example.org/ssd/assertion/e3e4dada2be24546bed3a7c639c201ae> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "SSDs deliver significantly higher read and write performance, with orders of magnitude of improvement in random-access input/output (I/O) operations." .

<http://example.org/ssd/assertion/f9ef9ec5fe184c88ae62a88615e8c105> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "25" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E4_EC1 ;
    dct:source "Higher operating temperatures can lead to higher failure rates, but modern SSDs employ throttling techniques that reduce the access rates to the underlying flash chips." .

ssd:hasTemperature a owl:DatatypeProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E2_MLC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "MLC_SSD_1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:operatesUnder ssd:E4_EC1 ;
    rdfs:isDefinedBy tax:SSD .

ssd:E3_TLC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "TLC_SSD_1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:operatesUnder ssd:E4_EC1 ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> rdfs:label "Reliability" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E4_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasTemperature "25" ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

