    rdfs:comment "improves rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_c6d9e90c a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_fcb9658b a owl:Axiom ;
    rdfs:comment "improvedBy rdfs:domain MetricsHealthAndState/Performance; rdfs:range xsd:decimal." .

