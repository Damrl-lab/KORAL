<http://example.org/ssd/assertion/2e989185504e446098aab6824b7266c5> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "The experimental results show the proposed scheme consistently improve the overall latency for various workloads." .

<http://example.org/ssd/assertion/7180c204e2fa4b1c95418b248ccb3119> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E5_WP1 ;
    dct:source "Both trace files had a big ratio of random I/O requests." .

<http://example.org/ssd/assertion/735720984d6445d5989db4493d6aef21> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Sequential> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E5_WP1 ;
    dct:source "In the case of MongoDB and MySQL, the ratio of sequential I/O is high." .

<http://example.org/ssd/assertion/9bebf6564f3c46359532550ce536df73> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E4_EC1 ;
    dct:source "The experimental results show the proposed scheme consistently improve the overall latency for various workloads." .

<http://example.org/ssd/assertion/e8b2830a8cd34bedb76897d356e886a9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E5_WP1 ;
    dct:source "Cassandra, MongoDB, and RocksDB corresponding to the YCSB series had a higher read ratio than write." .

<http://example.org/ssd/assertion/f270c73506c1407393f8b772cb60c0a0> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:degrades ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    dct:source "The GC is the major source of performance degradation because it greatly increases the latency for SSDs." .

ssd:degrades a owl:ObjectProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

