ssd:axiom_345a284c a owl:Axiom ;
    rdfs:comment "degradedBy rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic/MediaManagement." .

ssd:axiom_53bbd3cd a owl:Axiom ;
    rdfs:comment "improves rdfs:domain InternalFirmwareAndLogic/MediaManagement; rdfs:range InternalFirmwareAndLogic/DataPath/Error Correction (ECC)." .

ssd:axiom_82b416cb a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_9dbac704 a owl:Axiom ;
    rdfs:comment "impacts rdfs:domain InternalFirmwareAndLogic/MediaManagement; rdfs:range MetricsHealthAndState/Endurance." .

ssd:axiom_ed95377c a owl:Axiom ;
    rdfs:comment "improves rdfs:domain InternalFirmwareAndLogic/MediaManagement; rdfs:range MetricsHealthAndState/Endurance." .

ssd:axiom_fda863e9 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

