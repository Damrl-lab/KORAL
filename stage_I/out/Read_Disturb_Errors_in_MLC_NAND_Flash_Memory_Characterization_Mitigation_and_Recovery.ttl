<http://example.org/ssd/assertion/03955d0065b743d79c1a0fcbb26da842> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    ssd:predicate ssd:degradedBy ;
    ssd:subject ssd:E1_MLC_NAND_Flash ;
    dct:source "A key contributor to this reduced reliability is read disturb, where a read to one row of cells impacts the threshold voltages of unread flash cells in different rows of the same block." .

<http://example.org/ssd/assertion/0ada370668674551943235eb19b85336> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E8_Read_Disturb_Recovery__RDR_ ;
    dct:source "Our evaluations show that this recovery technique reduces the raw bit error rate by 36%." .

<http://example.org/ssd/assertion/458a63d2e1bd4e4d955ad1b781100d9a> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We evaluate Vpass Tuning with I/O traces collected from a wide range of real workloads with different use cases." .

<http://example.org/ssd/assertion/926824bff3524812a7c27141e53b4fba> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_MLC_NAND_Flash ;
    dct:source "NAND flash memory currently sees widespread usage as a storage device, having been incorporated into systems ranging from mobile devices and client computers to datacenter storage." .

<http://example.org/ssd/assertion/97bad18e1cc94d7789bdaf28e1737190> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E7_Vpass_Tuning ;
    dct:source "Using real workload traces, our evaluations show that this technique increases flash memory endurance by an average of 21%." .

<http://example.org/ssd/assertion/fd06eb2c62f94e13a47e64a7ca16181f> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:impacts ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    dct:source "Such disturbances may shift the threshold voltages of these unread cells to different logical states than originally programmed, leading to read errors that hurt endurance." .

ssd:E7_Vpass_Tuning a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement>,
        owl:Thing ;
    rdfs:label "Vpass Tuning" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> .

ssd:E8_Read_Disturb_Recovery__RDR_ a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement>,
        owl:Thing ;
    rdfs:label "Read Disturb Recovery (RDR)" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> .

