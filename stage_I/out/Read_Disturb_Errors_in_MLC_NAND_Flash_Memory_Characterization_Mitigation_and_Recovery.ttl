ssd:E1_MLC_NAND_Flash a tax:SSD,
        owl:Thing ;
    rdfs:label "MLC_NAND_Flash" ;
    ssd:degradedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

