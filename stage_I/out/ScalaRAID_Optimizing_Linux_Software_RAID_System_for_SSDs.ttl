<http://example.org/ssd/assertion/70d4820c7ef2402883292e2c4d324497> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_ScalaRAID ;
    dct:source "ScalaRAID can improve throughput by 89.4% while decreasing 99.99th percentile latency by 85.4% compared to mdraid." .

<http://example.org/ssd/assertion/aadd0295247849e187b7094e87b17a11> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_ScalaRAID ;
