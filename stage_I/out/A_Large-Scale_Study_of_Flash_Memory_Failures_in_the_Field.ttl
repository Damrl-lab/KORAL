@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_de3b8eca a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:axiom_f05f12d3 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/018316e61d09418cbcf8bd0d6ea00a46> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> ;
    dct:source "Higher temperatures lead to higher failure rates, but techniques that throttle SSD operation appear to greatly reduce the negative reliability impact of higher temperatures." .

<http://example.org/ssd/assertion/0d0d9f441e1a401f9811533fdc3490cb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E7_SSD1 ;
    dct:source "We examined a range of high-capacity multi-level cell (MLC) flash-based SSDs with capacities of 720 GB, 1.2 TB, and 3.2 TB." .

<http://example.org/ssd/assertion/2b690bc502cd4b5f945e503e1789c88c> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "40" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E6_EC1 ;
    dct:source "We find that at an operating temperature range of 30 to 40Â°C, SSDs across server platforms see similar failure rates or slight increases in failure rates as temperature increases." .

<http://example.org/ssd/assertion/b8a4f72410a54e6d88139ec46e1390c5> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E8_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E7_SSD1 ;
    dct:source "Our study considers a variety of SSD characteristics, including: the amount of data written to and read from flash chips; how data is mapped within the SSD address space; the amount of data copied, erased, and discarded by the flash controller; and flash board temperature and bus power." .

<http://example.org/ssd/assertion/bc35afa55d5b466091372fa08a45beb6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E6_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E7_SSD1 ;
    dct:source "Servers use flash memory based solid state drives (SSDs) as a high-performance alternative to hard disk drives to store persistent data." .

<http://example.org/ssd/assertion/d28990c975834853918c1e5407960221> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E6_EC1 ;
    dct:source "We analyze data collected across a majority of flash-based solid state drives at Facebook data centers over nearly four years and many millions of operational hours." .

ssd:correlatesWith a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E8_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> rdfs:label "UBER" ;
    ssd:correlatesWith <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> .

ssd:E7_SSD1 a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:hasWorkloadProfile ssd:E8_WP1 ;
    ssd:operatesUnder ssd:E6_EC1 ;
    rdfs:isDefinedBy tax:SSD .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> .

ssd:E6_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasTemperature "40" ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

