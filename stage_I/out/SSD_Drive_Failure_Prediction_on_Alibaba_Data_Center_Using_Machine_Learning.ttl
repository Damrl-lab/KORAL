@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_1d017a80 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_2bdef0ec a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_48358ceb a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_5097a2c0 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_57124713 a owl:Axiom ;
    rdfs:comment "usesSMARTAttribute rdfs:domain SSD; rdfs:range SMARTAttribute." .

ssd:axiom_79e960c9 a owl:Axiom ;
    rdfs:comment "includes rdfs:domain SMARTAttribute; rdfs:range SMARTAttribute." .

ssd:axiom_bc09f176 a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/06609cab0cab4d1baa104d4a432f88f4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E6_SSD_Failure_Prediction_System ;
    dct:source "Alibaba collects daily SMART data of 3D TLC 4TB SSD drives from February to November in 2021 and provides them to Solidigm." .

<http://example.org/ssd/assertion/0a1d27db96da4a74acc57d8dec0be104> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Unsafe%2520Shutdown%2520Count%2520%2520%255BID%253A%2520N/A%255D> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    dct:source "Processing the data, we found that the following attributes correlate most strongly with drive failure prediction: (1) Number of unsafe shutdown: Failed drives have roughly twice as many unsafe shutdown as healthy drives." .

<http://example.org/ssd/assertion/413e7d1ba8354a65b6517361313fb162> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:predicate ssd:usesSMARTAttribute ;
    ssd:subject ssd:E6_SSD_Failure_Prediction_System ;
    dct:source "Solidigm and Alibaba collaborate to collect and analyze Self-Monitoring, Analysis, and Reporting Technology (SMART) data and predict SSD failures 30 days ahead of time using machine learning techniques." .

<http://example.org/ssd/assertion/9a4a86d6e68344c9985cee97b2383462> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E8_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E7_EC1 ;
    dct:source "Studies that targeted solid-state drives' failures are mainly in a controlled laboratory environment, trying to understand specific errors and finding correlations with specific workloads." .

<http://example.org/ssd/assertion/c0f291ae15f94c1f9795cb77e710841d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E6_SSD_Failure_Prediction_System ;
    dct:source "In this paper, we study general SSD failure prediction in large-scale production environments and optimize our machine learning models with the highest Precision instead of the highest F1-score to minimize false positive rate, with the goal of deploying the system in the data center." .

<http://example.org/ssd/assertion/eb6d7ce7714c424b9b3f3983244005da> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E7_EC1 ;
    dct:source "Flash-based Solid-State Drives (SSDs) have become more and more important in data centers and enterprise storage systems." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E8_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E6_SSD_Failure_Prediction_System a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD_Failure_Prediction_System" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:operatesUnder ssd:E7_EC1 ;
    ssd:usesSMARTAttribute <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    rdfs:isDefinedBy tax:SSD .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Unsafe%2520Shutdown%2520Count%2520%2520%255BID%253A%2520N/A%255D> rdfs:label "Unsafe Shutdown Count" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Unsafe%2520Shutdown%2520Count%2520%2520%255BID%253A%2520N/A%255D> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "3D TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E7_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E8_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> rdfs:label "SMARTAttribute" ;
    ssd:includes <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/Unsafe%2520Shutdown%2520Count%2520%2520%255BID%253A%2520N/A%255D> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> .

