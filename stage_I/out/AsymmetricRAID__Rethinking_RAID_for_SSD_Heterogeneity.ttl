@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_4717e205 a owl:Axiom ;
    rdfs:comment "hasAccessPattern rdfs:domain WorkloadProfile; rdfs:range AccessPattern." .

ssd:axiom_5370390d a owl:Axiom ;
    rdfs:comment "hasBlockSize rdfs:domain WorkloadProfile; rdfs:range xsd:decimal." .

ssd:axiom_7d3ff9c8 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_aaa829d3 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_b98dee4b a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/152c036a965c4eadbcf1ee1e7fa29788> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    dct:source "The read throughput of the NVMe SSD outperforms the SATA SSD by 5.31× and 5.35× with 512 KiB and 1 MiB request sizes, respectively." .

<http://example.org/ssd/assertion/1eca8a06011f4111876294738b5b6f17> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E9_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E8_EC1 ;
    dct:source "We measure the latency distribution under read-only I/O workloads (i.e., 4 KiB sequential read) every morning when the server is idle." .

<http://example.org/ssd/assertion/221b52b09a154ad19617d1a5614edb3c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:degrades ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> ;
    dct:source "The read throughput of the NVMe SSD outperforms the SATA SSD by 5.31× and 5.35× with 512 KiB and 1 MiB request sizes, respectively." .

<http://example.org/ssd/assertion/341cd503115c4dbfba27c6df0471ac66> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E8_EC1 ;
    dct:source "The explosive growth of data, driven by the era of big data, places significant stress on data center storage systems." .

<http://example.org/ssd/assertion/6db492951d2448fbbc23f967d0ef9ebb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E8_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_Asymmetric_RAID ;
    dct:source "The explosive growth of data, driven by the era of big data, places significant stress on data center storage systems." .

<http://example.org/ssd/assertion/808478605b484cb2bd74dca6e2f4cbfe> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "4" ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E9_WP1 ;
    dct:source "We measure the latency distribution under read-only I/O workloads (i.e., 4 KiB sequential read) every morning when the server is idle." .

<http://example.org/ssd/assertion/b233b4734eda4faea8e0a3cdb043ec78> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_Asymmetric_RAID ;
    dct:source "Asym-RAID asymmetrically distributes data across the array to fully utilize the capacity of each SSD." .

<http://example.org/ssd/assertion/e0b0aa76353949e191f0a444d9c6135b> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_Asymmetric_RAID ;
    dct:source "Recent studies have demonstrated that fail-slow drives can cause latency spikes of up to 3.65× in large-scale storage clusters." .

ssd:degrades a owl:ObjectProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> rdfs:label "NVMe" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> rdfs:label "SATA" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> .

ssd:E2_Asymmetric_RAID a <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy>,
        owl:Thing ;
    rdfs:label "Asymmetric-RAID" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E8_EC1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy> .

ssd:E9_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    ssd:hasBlockSize "4" ;
    ssd:unit "KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

ssd:E8_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E9_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

