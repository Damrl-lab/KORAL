    ssd:object tax:SSD ;
    ssd:predicate ssd:isImplementedIn ;
    ssd:subject ssd:E5_FLIN ;
    dct:source "FLIN is implemented fully within the SSD controller firmware, requiring no new hardware, and has negligible (<0.06%) storage cost." .

<http://example.org/ssd/assertion/fd6a75a9e4d643f690a0d249d7093227> a ssd:Assertion ;
