    ssd:subject ssd:E3_QLC_SSD_1 ;
    dct:source "The paper analyzes various datasets including internal qualification methods and field studies." .

<http://example.org/ssd/assertion/a803640669d54798b2675ecbfda3250e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
