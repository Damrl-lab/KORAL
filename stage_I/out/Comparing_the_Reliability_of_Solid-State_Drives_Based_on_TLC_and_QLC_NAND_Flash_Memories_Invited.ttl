<http://example.org/ssd/assertion/c3602da69cf64044b1c9b0b59f2edf3b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "A typical enterprise SSD in 2023 uses Triple-Level Cell (TLC), a misnomer that refers to eight possible VT states to hold three bits of data per cell." .

<http://example.org/ssd/assertion/d6705242a7e24176845dcf42de37a52e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E8_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
