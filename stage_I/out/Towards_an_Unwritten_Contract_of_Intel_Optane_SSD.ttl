<http://example.org/ssd/assertion/2073d312c62048b0b0912e8df1977679> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "Optane SSD has lower latency than Flash SSD when request size and queue depth are small." .

<http://example.org/ssd/assertion/24ab93750e81485a8774d95b0effad1c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "We analyze a NVM-based block device – the Intel Optane SSD – and formalize an “unwritten contract” of the Optane SSD." .

<http://example.org/ssd/assertion/27df8a42f84d4d4a9e4d1aa359570337> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "However, when QD >8 and request size >16KB, Flash SSD achieves lower latency, by as much as 40%." .

<http://example.org/ssd/assertion/2a01b4ca131447cda810b51a88ccf3d2> a ssd:Assertion ;
    ssd:confidence 0.85 ;
