<http://example.org/ssd/assertion/8dace03bafd248448e2bc5deca45b615> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "Optane SSD can achieve peak throughput when serving small requests with low queue depth." .

<http://example.org/ssd/assertion/d3ccdc8ac43949989a5a9d622fa929bc> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "Optane SSD is a random access block device, where clients can observe the same performance for random and sequential workloads." .

