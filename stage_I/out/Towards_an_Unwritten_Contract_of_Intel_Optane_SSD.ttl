<http://example.org/ssd/assertion/31cf7be430c54a9e9f5c48a06e86af34> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "Optane SSD offers numerous opportunities for applications. For example, Intel’s Memory Direct Technology (IMDT) enables the use of Optane SSD as a DRAM alternative." .

<http://example.org/ssd/assertion/484fd971dc98463f9fbbb50f4aafb28c> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "The limited internal parallelism of the Optane SSD impacts its throughput in two ways." .

<http://example.org/ssd/assertion/6189721e189a445dbb2e1205baf651a1> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Sequential> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "Optane SSD is a random access block device, where clients can observe the same performance for random and sequential workloads." .

<http://example.org/ssd/assertion/7ba75de45d604b32b9f684ea72be996e> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD64> ;
