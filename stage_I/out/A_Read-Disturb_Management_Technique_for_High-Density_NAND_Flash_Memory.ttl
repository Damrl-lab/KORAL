@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_2c1b4343 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_7fc7b77c a owl:Axiom ;
    rdfs:comment "hasReadWriteMix rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile/ReadWriteMix." .

ssd:axiom_d16a5a55 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain InternalFirmwareAndLogic/MediaManagement; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_d73ffbb5 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_e4aa27bc a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/149d2b3a5dfa4620bf94073df97086dd> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_SSD ;
    dct:source "For a block in TLC NAND flash memory, the maximum read count may be just about 40,000." .

<http://example.org/ssd/assertion/207d91628deb4ded8c64de9ff505aa5f> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "In our observation, for many read dominant applications such as a web search engine server, an ad server, and a database server, a large number of read reclaims still occur." .

<http://example.org/ssd/assertion/75f6465ec7ba4cfba2af5fcb4400446b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Our experimental results show that our technique is effective in handling the read-disturb problem, reducing the time overhead of data migrations on average by 50% over an FTL based on the existing read-disturb management technique." .

<http://example.org/ssd/assertion/89758a128d3d46c7bebe1c3007321974> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E4_Read_Disturb_Management ;
    dct:source "Our technique proactively converts highly skewed read accesses to a small number of blocks into more balanced read accesses to a large number of blocks by intelligently changing data block locations accessed." .

<http://example.org/ssd/assertion/cd3e1fd1f87948adbd874ef7ec1012d8> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "In our observation, for many read dominant applications such as a web search engine server, an ad server, and a database server, a large number of read reclaims still occur." .

ssd:E4_Read_Disturb_Management a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement>,
        owl:Thing ;
    rdfs:label "Read-Disturb Management" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> rdfs:label "Enterprise Server" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> rdfs:label "Read-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

