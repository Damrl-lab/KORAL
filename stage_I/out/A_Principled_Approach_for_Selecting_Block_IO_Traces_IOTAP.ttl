<http://example.org/ssd/assertion/1aa9025c710f4682a66546644d23eb15> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "The feature that discerns traces the most is the root-mean-square (RMS) of distances between two consecutive read I/Os." .

<http://example.org/ssd/assertion/24c522d30d394402bb9714a5b00b3158> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "The feature that discerns traces the most is the root-mean-square (RMS) of distances between two consecutive read I/Os." .

<http://example.org/ssd/assertion/3038b42f62784941b5c468be5571bb35> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_NVMe_SSD ;
    dct:source "We demonstrate the usefulness of IOTap by comparing its results with experiments on real SSDs, achieving a high correlation of 0.92 for an NVMe SSD." .

<http://example.org/ssd/assertion/3dcb49cba8534b2aad4e8bef3dd62863> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "The feature that discerns traces the most is the root-mean-square (RMS) of distances between two consecutive read I/Os." .

<http://example.org/ssd/assertion/8671a81f66604fa3a44e224827f7d1a3> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    dct:source "We use I/O latencies as measurable behavior, and demonstrate that our hypothesis holds true." .

<http://example.org/ssd/assertion/a30bad76ee2d4424a5b9d19938993351> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "The feature that discerns traces the most is the root-mean-square (RMS) of distances between two consecutive read I/Os." .

<http://example.org/ssd/assertion/e8eb36f3f23c48f482e94e0f5ee96ab6> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "We present IOTap, a tool that extracts important features from traces and computes (dis)similarities among them to provide a guideline for selecting traces when benchmarking storage systems." .

ssd:E2_NVMe_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "NVMe SSD" ;
    ssd:operatesUnder ssd:E3_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

