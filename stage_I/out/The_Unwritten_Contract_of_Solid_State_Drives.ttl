@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_34708bb6 a owl:Axiom ;
    rdfs:comment "isA rdfs:domain File System; rdfs:range Class." .

ssd:axiom_6bece1f2 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_bbe03974 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_d05fb023 a owl:Axiom ;
    rdfs:comment "hasAccessPattern rdfs:domain WorkloadProfile; rdfs:range xsd:string." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/08a9d5bfc6234f98b83b894fcb26ea59> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "SSD clients should create data with similar lifetimes (Uniform Data Lifetime rule)." .

<http://example.org/ssd/assertion/0a855f6ab351446d9548ac74ec4e78ae> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "Sequential" ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "Sequential writes, often enabled by log structure, are believed to reduce garbage collection overhead inside SSDs." .

<http://example.org/ssd/assertion/152591d9111b4b428418d18622713113> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:predicate ssd:isA ;
    ssd:subject ssd:E5_ext4 ;
    dct:source "We study the performance of four applications – LevelDB, RocksDB, SQLite, and Varmail – running atop a range of modern file systems – Linux ext4, XFS, and the flash-friendly F2FS." .

<http://example.org/ssd/assertion/3dd6b4aa00bd4ffab48380780f8fc94a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "We utilize this contract to study application and file system pairings atop a range of SSDs." .

<http://example.org/ssd/assertion/430405b1523e4909bf090eb477fae8d5> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We utilize this contract to study application and file system pairings atop a range of SSDs." .

<http://example.org/ssd/assertion/5abc039cb9924704bdee675a6bb36676> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "Clients of SSDs should start writing at the aligned beginning of a block boundary and write sequentially (Aligned Sequentiality rule)." .

<http://example.org/ssd/assertion/864baf6007b042d2b276b7684e7878a3> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "To exploit the internal parallelism of SSDs, SSD clients should issue large requests or many outstanding requests (Request Scale rule)." .

<http://example.org/ssd/assertion/864e65bc65814a2480a1a68cf4b7e3ff> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:predicate ssd:isA ;
    ssd:subject ssd:E7_F2FS ;
    dct:source "We study the performance of four applications – LevelDB, RocksDB, SQLite, and Varmail – running atop a range of modern file systems – Linux ext4, XFS, and the flash-friendly F2FS." .

<http://example.org/ssd/assertion/897529cba8f74e828eb98c7cf634d766> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "SSD clients should group writes by the likely death time of data (Grouping By Death Time rule)." .

<http://example.org/ssd/assertion/9308db8561c84185a92cb6cd50996a55> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "Random" ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "Random writes are often considered harmful for SSDs because they are believed to increase garbage collection overhead." .

<http://example.org/ssd/assertion/a8197e3f85604a78a926c47af8926ac7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:predicate ssd:isA ;
    ssd:subject ssd:E6_XFS ;
    dct:source "We study the performance of four applications – LevelDB, RocksDB, SQLite, and Varmail – running atop a range of modern file systems – Linux ext4, XFS, and the flash-friendly F2FS." .

<http://example.org/ssd/assertion/d83845c8680640459878daf3598d24e5> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "SSDs should be accessed with locality (Locality rule)." .

ssd:E5_ext4 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "ext4" ;
    ssd:isA <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:E6_XFS a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "XFS" ;
    ssd:isA <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:E7_F2FS a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "F2FS" ;
    ssd:isA <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasWorkloadProfile ssd:E3_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E3_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasAccessPattern "Random",
        "Sequential" ;
    ssd:unit "AccessPattern" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:improves a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> rdfs:label "Aligned Sequentiality",
        "Grouping by Death Time",
        "Locality",
        "Request Scale",
        "Uniform Data Lifetime" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> .

