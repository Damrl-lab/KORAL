@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_2568b520 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_2fd8476a a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_38e92b80 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_55e5c1b7 a owl:Axiom ;
    rdfs:comment "hasAccessPattern rdfs:domain WorkloadProfile; rdfs:range AccessPattern." .

ssd:axiom_c6c0ac99 a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:axiom_ee5db399 a owl:Axiom ;
    rdfs:comment "impactedBy rdfs:domain SSD; rdfs:range Reliability." .

ssd:axiom_f4719323 a owl:Axiom ;
    rdfs:comment "hasReadWriteMix rdfs:domain WorkloadProfile; rdfs:range ReadWriteMix." .

ssd:axiom_f48f031f a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/1030bee8761040a7908150cd2cdedad3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E3_EC1 ;
    dct:source "NVMe SSD has become a staple in modern datacenters thanks to its high throughput and ultra-low latency." .

<http://example.org/ssd/assertion/2cb835fca31e4d80a1f802a21897e6a4> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "30" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E3_EC1 ;
    dct:source "The denser bits per cell (i.e., TLC) shows lower reliability and the vertical stacking (i.e., 3D flash) can exhibit disparate behaviors or even opposite patterns (e.g., lower error rate under higher temperatures)." .

<http://example.org/ssd/assertion/3580a50f423249cc9c3db4f6340a97fe> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_NVMe_SSD_1 ;
    dct:source "NVMe SSD has become a staple in modern datacenters thanks to its high throughput and ultra-low latency." .

<http://example.org/ssd/assertion/3ec98411ad6c4a76a2372d87385e15b0> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E6_Fail_Stop_Failure ;
    ssd:predicate ssd:impactedBy ;
    ssd:subject ssd:E1_NVMe_SSD_1 ;
    dct:source "We study the fail-stop and fail-slow failures of NVMe SSDs deployed at Alibaba." .

<http://example.org/ssd/assertion/409723f284524ad1a1740ce817ffca8c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_Fail_Slow_Failure ;
    ssd:predicate ssd:impactedBy ;
    ssd:subject ssd:E1_NVMe_SSD_1 ;
    dct:source "More importantly, we discover that the ultra-low latency nature makes NVMe SSD much more likely to be impacted by fail-slow failures." .

<http://example.org/ssd/assertion/44b669ff5980417aa8f10f74fd58bc2d> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E8_WP1 ;
    dct:source "A higher write amplification factor (i.e., NAND writes size divided by logical writes size) therefore indicates a more random and small-writes-dominant workload." .

<http://example.org/ssd/assertion/4e2791efae884457b2a35b6ae90cfad5> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E8_WP1 ;
    dct:source "A higher write amplification factor (i.e., NAND writes size divided by logical writes size) therefore indicates a more random and small-writes-dominant workload." .

<http://example.org/ssd/assertion/63b61310e3f34074a9f89a754220bb99> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E8_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EC1 ;
    dct:source "A higher write amplification factor (i.e., NAND writes size divided by logical writes size) therefore indicates a more random and small-writes-dominant workload." .

<http://example.org/ssd/assertion/b1b83fd9bbed42848fe147cba3db56c3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_NVMe_SSD_1 ;
    dct:source "An outstanding example is the wide adoption of 3D-TLC NAND in NVMe SSD for larger capacity." .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:E5_Fail_Slow_Failure a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability>,
        owl:Thing ;
    rdfs:label "Fail-Slow Failure" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

ssd:E6_Fail_Stop_Failure a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability>,
        owl:Thing ;
    rdfs:label "Fail-Stop Failure" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> rdfs:label "Write-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "3D-TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E1_NVMe_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "NVMe_SSD_1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:impactedBy ssd:E5_Fail_Slow_Failure,
        ssd:E6_Fail_Stop_Failure ;
    ssd:operatesUnder ssd:E3_EC1 ;
    rdfs:isDefinedBy tax:SSD .

ssd:E8_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    ssd:hasAccessPattern <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E3_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasTemperature "30" ;
    ssd:hasWorkloadProfile ssd:E8_WP1 ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

