ssd:axiom_d4d97aff a owl:Axiom ;
    rdfs:comment "hasEndurance rdfs:domain SSD; rdfs:range MetricsHealthAndState/Endurance." .

ssd:axiom_e32ab602 a owl:Axiom ;
    rdfs:comment "uses rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic/DataPath/Error Correction (ECC)." .

ssd:axiom_f349aedb a owl:Axiom ;
