<http://example.org/ssd/assertion/1f123401716e4560911ea739684c3203> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E10_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_LightWarner ;
    dct:source "The performance of LightWarner on six types of 3D flash chips." .

<http://example.org/ssd/assertion/56f7fdd0090347e0996027615c4bf74f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:uses ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "These errors cannot be corrected by ECC." .

<http://example.org/ssd/assertion/832389da57f04d3cb7a5b0c940e5d6cf> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E8_RBER ;
    ssd:predicate ssd:hasRBER ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "The variation of RBER provides information related to NAND flash memory reliability degradation." .

<http://example.org/ssd/assertion/852e6d37b02149a0af12ce08e3b2e3eb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:hasReliability ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "In 3D NAND flash memory, the reliability of storage cell is related to various factors, such as the temperature, the retention time, and the process variation." .

<http://example.org/ssd/assertion/8e02cd1a3d3a46cbb126bbb0bb885933> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_P_E_Cycles ;
    ssd:predicate ssd:hasEndurance ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "The minimum number of P/E cycles that flash memory can withstand before a failure is the endurance of the flash memory." .

<http://example.org/ssd/assertion/a119b9176aed478b9f918c68b3d68095> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E9_Uncorrectable_Error ;
    ssd:predicate ssd:hasUncorrectableError ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    dct:source "We take the emergence of uncorrectable errors when read operation as the failure in flash memory." .

<http://example.org/ssd/assertion/a1a5f8cf84cd4e3f8bdd4378c01ee81f> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors> ;
    ssd:predicate ssd:hasRetentionTime ;
    ssd:subject ssd:E10_EnvironmentalAndOperationalContext ;
    dct:source "The retention test stops when the retention time achieves 15.7 h." .

<http://example.org/ssd/assertion/da50d59177dd4147831db071388b3d83> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "70" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E10_EnvironmentalAndOperationalContext ;
    dct:source "We simulate the retention loss of 180 days at room temperature by baking flash chips under 70Â°C." .

ssd:E2_LightWarner a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "LightWarner" ;
    ssd:operatesUnder ssd:E10_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

