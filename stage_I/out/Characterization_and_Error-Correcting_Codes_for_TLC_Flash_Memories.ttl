<http://example.org/ssd/assertion/08822d682bd24564aea813f6b00115a2> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_TLC_SSD_1 ;
    dct:source "To address the observed error behavior, a new error-correcting scheme for TLC flash is given and is compared with BCH and LDPC codes." .

<http://example.org/ssd/assertion/2bca661d7e0e4593bc4a09cffd08756d> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_TLC_SSD_1 ;
    dct:source "As mentioned in [15], we note that these experiments were conducted in a controlled laboratory environment." .

<http://example.org/ssd/assertion/3c498669b6f6468e8e4ec6785989426c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> ;
    ssd:predicate ssd:hasReliability ;
    ssd:subject ssd:E1_TLC_SSD_1 ;
    dct:source "We first measured the average BER as a function of the number of program/erase cycles for several TLC blocks." .

<http://example.org/ssd/assertion/7f03f02feb554706af814255911de217> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_TLC_SSD_1 ;
    dct:source "In this work, the error behavior of TLC flash is studied through an empirical database of errors." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> rdfs:label "Error Correction (ECC)" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> rdfs:label "BER" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E1_TLC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "TLC_SSD_1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasReliability <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    ssd:uses <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    rdfs:isDefinedBy tax:SSD .

