ssd:axiom_1ee6d1f8 a owl:Axiom ;
    rdfs:comment "hasReliability rdfs:domain SSD; rdfs:range MetricsHealthAndState/Reliability." .

ssd:axiom_88bb0766 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

