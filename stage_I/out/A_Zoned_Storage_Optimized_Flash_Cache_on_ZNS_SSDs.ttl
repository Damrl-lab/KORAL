<http://example.org/ssd/assertion/069aaccc5cb741e7a85e1269c887a0ef> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E7_WorkloadProfile ;
    dct:source "The cache workloads have heavy insertions, updates, and evictions and cause random write of blocks." .

<http://example.org/ssd/assertion/0c49566c0af1482aa1f2b9391fd729cc> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E7_WorkloadProfile ;
    dct:source "The cache workloads have heavy insertions, updates, and evictions and cause random write of blocks." .

<http://example.org/ssd/assertion/2313aed59ab344ef9bab49f3989b534f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E10_1077MiB ;
    ssd:predicate ssd:hasZoneSize ;
    ssd:subject ssd:E2_ZNS_SSD ;
    dct:source "ZNS SSDs organize the storage space as large zones (e.g., 96 MiB of Samsung ZNS SSD and 1077 MiB of Western Digital SSDs)." .

<http://example.org/ssd/assertion/3ac979416a564f428e50cc72a01aea71> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "The cache workloads have heavy insertions, updates, and evictions and cause random write of blocks." .

<http://example.org/ssd/assertion/a14634c63f174017aa19bf206dad0a27> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_Z_CacheLib ;
    dct:source "Z-CacheLib can achieve up to 6X throughput and 92% WA reduction compared with F2FS-based scheme." .

<http://example.org/ssd/assertion/d68ae9a391124a52b13520e1db90a094> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Z_CacheLib ;
    dct:source "Z-CacheLib can achieve up to 6X throughput and 92% WA reduction compared with F2FS-based scheme." .

<http://example.org/ssd/assertion/d71ebfeb90774dfeb974eddb2af0b28c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:reduces ;
    ssd:subject ssd:E1_Z_CacheLib ;
    dct:source "Z-CacheLib can achieve up to 6X throughput and 92% WA reduction compared with F2FS-based scheme." .

<http://example.org/ssd/assertion/ffa3d523d2844ab681911400e1b188b2> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:maintains ;
    ssd:subject ssd:E1_Z_CacheLib ;
    dct:source "The cache hit ratio of Z-CacheLib is only about 0.1% to 0.3% lower than that of regular SSD and ZNS-F2FS schemes." .

ssd:E2_ZNS_SSD a <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS>,
        owl:Thing ;
    rdfs:label "ZNS SSD" ;
    ssd:hasZoneSize ssd:E10_1077MiB ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> .

