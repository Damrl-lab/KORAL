<http://example.org/ssd/assertion/04247c869e934507a2212d1069adc45e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_3D_CT_TLC_NAND_Flash ;
    dct:source "To improve the memory reliability of 3D CT TLC NAND, different intervals between P&E operations should be adopted considering the operating temperatures." .

<http://example.org/ssd/assertion/1a7132710296473ab165d60ed9603af1> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_CT_TLC_NAND_Flash ;
    dct:source "Three-dimensional charge-trapping (CT) NAND flash memory has attracted extensive attention owing to its unique merits, including huge storage capacities, large memory densities, and low bit cost." .

<http://example.org/ssd/assertion/361c81d0b7774a27be17b8b2f9e25d60> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "85" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "The tester can support a maximum data transfer of ~200 MHz and a wide temperature range for operations, from 25째C (room temperature, RT) to 85째C (high temperature, HT)." .

<http://example.org/ssd/assertion/6159a374340841a882b22a1582ef81fb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "25" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "The tester can support a maximum data transfer of ~200 MHz and a wide temperature range for operations, from 25째C (room temperature, RT) to 85째C (high temperature, HT)." .

<http://example.org/ssd/assertion/71c91ddaf562495e9597c9e6bacb4dd7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_3D_CT_TLC_NAND_Flash ;
    dct:source "The impacts of the intervals between P&E operations on 3D TLC NAND flash are investigated under room temperature and high temperature." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:E1_3D_CT_TLC_NAND_Flash a tax:SSD,
        owl:Thing ;
    rdfs:label "3D CT TLC NAND Flash" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> rdfs:label "Reliability" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasTemperature "25",
        "85" ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

