@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_4ac9b625 a owl:Axiom ;
    rdfs:comment "hasNANDWrites rdfs:domain SSD; rdfs:range SSD/MetricsHealthAndState/SMARTAttribute/NAND Writes (1GiB)  [ID: 249]." .

ssd:axiom_60d8c583 a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:axiom_ad872134 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType." .

ssd:axiom_da2345d7 a owl:Axiom ;
    rdfs:comment "hasUncorrectableErrors rdfs:domain SSD; rdfs:range SSD/MetricsHealthAndState/Reliability/UBER." .

ssd:axiom_e3e55a58 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_eaa1be23 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain SSD; rdfs:range SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/22e387843ced424aaae4738b9f2273cc> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "we then conducted the endurance test and collected the reliability data for the entire life cycle of the 3D TLC SSD in the laboratory environment with reference to the JEDEC standard." .

<http://example.org/ssd/assertion/2989977f93ff44dea314ff377a43cab8> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E1_SSD ;
    dct:source "The priority of 3D TLC SSDs in data center storage is becoming higher and higher." .

<http://example.org/ssd/assertion/704925bea03d4a20b0e02052fdfcb237> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_SSD ;
    dct:source "we then studied lifetime SSD reliability characteristics based on 1.5 PB of data." .

<http://example.org/ssd/assertion/720cb5d25bd548a1b2e424c94b9555e2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> ;
    ssd:predicate ssd:hasUncorrectableErrors ;
    ssd:subject ssd:E1_SSD ;
    dct:source "The uncorrectable errors do not occur immediately when SSDs are put into use and they will suddenly increase to a large number when an SSD is close to its endurance limit." .

<http://example.org/ssd/assertion/886dffdc9cae4e7db7f4bf8f2309ca6c> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:objectLiteral "50" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E7_EnvironmentalAndOperationalContext ;
    dct:source "For a long period of time, the temperature fluctuates steadily in a range of 40°C–50°C." .

<http://example.org/ssd/assertion/8fa4c019aacd49e5b61fe6771722eef1> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/NAND%2520Writes%2520%25281GiB%2529%2520%2520%255BID%253A%2520249%255D> ;
    ssd:predicate ssd:hasNANDWrites ;
    ssd:subject ssd:E1_SSD ;
    dct:source "The accumulative P/E cycles of an SSD can be estimated according to the amount of data written to an SSD." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:E7_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasTemperature "50" ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> rdfs:label "Uncorrectable Errors" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/NAND%2520Writes%2520%25281GiB%2529%2520%2520%255BID%253A%2520249%255D> rdfs:label "NAND Writes" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/NAND%2520Writes%2520%25281GiB%2529%2520%2520%255BID%253A%2520249%255D> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "3D TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasNANDWrites <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/NAND%2520Writes%2520%25281GiB%2529%2520%2520%255BID%253A%2520249%255D> ;
    ssd:hasUncorrectableErrors <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability/UBER> ;
    ssd:operatesUnder ssd:E7_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

