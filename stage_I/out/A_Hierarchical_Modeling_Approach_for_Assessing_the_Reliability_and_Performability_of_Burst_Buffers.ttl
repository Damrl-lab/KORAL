    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:degradedBy ;
    ssd:subject tax:SSD ;
    dct:source "The failure of such SSDs reduces the performance of the BB and consequently increase the pressure over the parallel file systems." .

