ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/04f86be5b1fd471f87728cf2423174e7> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The features are normalized to have zero mean and unit variance." .

<http://example.org/ssd/assertion/2b9bf2d4b59c443ab417f1117350490f> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The features are normalized to have zero mean and unit variance." .

<http://example.org/ssd/assertion/5c54ed5fe987493d888fbbf94bb2e816> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The features are normalized to have zero mean and unit variance." .

<http://example.org/ssd/assertion/6367d47ef6cd4be2a6d30b41943d3bb7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "25" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Experimental Examples: We consider TLC NAND flash memory when the number of P/E cycle is 1 and the retention time varies from 25 to 50 hours." .

<http://example.org/ssd/assertion/96268d0540334fa88a27a36619f82bc7> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    dct:source "The proposed framework not only outperforms the conventional estimation schemes but also achieves the near-optimal frame error rate while sustaining low latency performance." .

<http://example.org/ssd/assertion/aebc3d9b71994e9ba092b4bf0a9984e8> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E5_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "The features are normalized to have zero mean and unit variance." .

<http://example.org/ssd/assertion/d0dac62532464373b78e144ab35a7f54> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_NAND_Flash_Memory ;
    dct:source "In triple-level cell (TLC) NAND flash memory (i.e., nD3), for example, three bits are allocated to most significant bit (MSB), central significant bit (CSB), and least significant bit (LSB) pages depending on their logical locations." .

<http://example.org/ssd/assertion/d158bd920d1e4e23a0b727a57c5776b7> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E5_WorkloadProfile ;
    dct:source "The features are normalized to have zero mean and unit variance." .

<http://example.org/ssd/assertion/e137753ce5ca49cf9f2e7ecba875b60b> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_NAND_Flash_Memory ;
    dct:source "Due to the sensitivity to the temperature, however, a flash memory controller is unable to acquire the exact knowledge of retention time." .

<http://example.org/ssd/assertion/ebca84ece5244b92909649c300986466> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_NAND_Flash_Memory ;
    dct:source "The proposed framework not only outperforms the conventional estimation schemes but also achieves the near-optimal frame error rate while sustaining low latency performance." .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

