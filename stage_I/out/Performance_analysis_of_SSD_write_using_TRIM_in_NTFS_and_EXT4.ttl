@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_2940f6f4 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic/Maintenance/TRIM; rdfs:range SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random." .

ssd:axiom_56901803 a owl:Axiom ;
    rdfs:comment "uses rdfs:domain SSD; rdfs:range SSD/InternalFirmwareAndLogic/Maintenance/TRIM." .

ssd:axiom_792b323b a owl:Axiom ;
    rdfs:comment "hasFileSystem rdfs:domain EnvironmentalAndOperationalContext; rdfs:range SSD/EnvironmentalAndOperationalContext/HostSystem/File System." .

ssd:axiom_7e217f1c a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic/Maintenance/TRIM; rdfs:range SSD/MetricsHealthAndState/Performance/Throughput." .

ssd:axiom_c8984452 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/2d8a93f4bf51446faf9815db29c14102> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_SSD ;
    dct:source "To maximize the write performance, SSD is used with the TRIM command." .

<http://example.org/ssd/assertion/3897e1a6e58c49f88c2cb31f51e7b00f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "The measurement carried out in two kinds of file system, NTFS and EXT4." .

<http://example.org/ssd/assertion/44434865a65a4dfb874d0a72d8cb3888> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    dct:source "In random write operation, enabling TRIM shows better performance than disabling TRIM." .

<http://example.org/ssd/assertion/8ac3a0ec8a5047219d4fae335e066b0b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_EXT4 ;
    ssd:predicate ssd:hasFileSystem ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "The measurement carried out in two kinds of file system, NTFS and EXT4." .

<http://example.org/ssd/assertion/e0510370cf824632972ae4eb472b7594> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    dct:source "In write operation, enabling TRIM shows better performance than disabling TRIM." .

<http://example.org/ssd/assertion/e330ccfc026f458f977ef9d40a2a63fe> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_NTFS ;
    ssd:predicate ssd:hasFileSystem ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "The measurement carried out in two kinds of file system, NTFS and EXT4." .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:uses <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    rdfs:isDefinedBy tax:SSD .

ssd:E4_EXT4 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "EXT4" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:E5_NTFS a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "NTFS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasFileSystem ssd:E4_EXT4,
        ssd:E5_NTFS ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> rdfs:label "TRIM" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> .

