<http://example.org/ssd/assertion/1490fc036bc249499c69fd02a3ff8aa9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/64KB> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E3_WP1 ;
    dct:source "The tests run with 16 concurrent threads and 64 outstanding requests per thread." .

<http://example.org/ssd/assertion/4aff4c4e89a848ff92195e7cb029eaba> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_HA_CSD ;
    dct:source "We implement HA-CSD in a commercial enterprise SSD with a code change of more than 25K lines in the host NVMe driver and SSD firmware." .

<http://example.org/ssd/assertion/5c61cb89db944feea7768ceeaae3b086> a ssd:Assertion ;
