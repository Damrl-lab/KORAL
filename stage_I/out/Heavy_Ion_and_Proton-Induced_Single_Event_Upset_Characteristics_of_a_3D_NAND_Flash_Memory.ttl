@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_22ca2885 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_2bea961a a owl:Axiom ;
    rdfs:comment "hasExternalFactor rdfs:domain EnvironmentalAndOperationalContext; rdfs:range Radiation." .

ssd:axiom_9a8a9ed8 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range MetricsHealthAndState/Reliability." .

ssd:axiom_d2595413 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/1cccfdef52a0454cace424fbb4be0404> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_3D_NAND_Flash ;
    dct:source "We evaluated the effects of heavy ion and proton irradiation for a 3D NAND flash." .

<http://example.org/ssd/assertion/75fce68c221d4963b2d76cf4eb89886c> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_NAND_Flash ;
    dct:source "The 3D NAND showed significantly reduced SEU susceptibility in single-level-cell (SLC) storage mode." .

<http://example.org/ssd/assertion/81b5f9f9e03a4c239f8d430b992f4b9b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Radiation> ;
    ssd:predicate ssd:hasExternalFactor ;
    ssd:subject ssd:E5_EnvironmentalAndOperationalContext ;
    dct:source "We evaluated the effects of heavy ion and proton irradiation for a 3D NAND flash." .

<http://example.org/ssd/assertion/9905e1e652ea42afa9beec3d7cfc0751> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_NAND_Flash ;
    dct:source "The 3D NAND showed similar single-event upset (SEU) sensitivity to a planar NAND of identical density in the multiple-cell level (MLC) storage mode." .

<http://example.org/ssd/assertion/acc2d209de1b48e7b9af94308f80ec26> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E6_SEU_Sensitivity ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_3D_NAND_Flash ;
    dct:source "The SLC mode of the 3D NAND offers significant improvement in the SEU tolerance relative to the MLC mode." .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E6_SEU_Sensitivity a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability>,
        owl:Thing ;
    rdfs:label "SEU Sensitivity" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:E5_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasExternalFactor <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Radiation> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Radiation> rdfs:label "Radiation" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Radiation> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> rdfs:label "SLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> .

ssd:E1_3D_NAND_Flash a tax:SSD,
        owl:Thing ;
    rdfs:label "3D NAND Flash" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC>,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> ;
    ssd:improves ssd:E6_SEU_Sensitivity ;
    ssd:operatesUnder ssd:E5_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

