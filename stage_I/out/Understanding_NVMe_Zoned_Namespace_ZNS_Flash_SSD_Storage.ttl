<http://example.org/ssd/assertion/5fbf927ff2314374b69c7a72500744ee> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_ZNS_SSD_1 ;
    dct:source "Achieving as low as single digit μ-second latency, several GB/s of bandwidth, and millions of I/O operations per second (IOPS)." .

<http://example.org/ssd/assertion/9183add5a9fb4118b33acce09c30283c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_ZNS_SSD_1 ;
    dct:source "Achieving as low as single digit μ-second latency, several GB/s of bandwidth, and millions of I/O operations per second (IOPS)." .

<http://example.org/ssd/assertion/973a7e5630204ed29f75f97cf921d5e6> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "Our main findings show that larger I/O sizes are required to saturate the ZNS device bandwidth." .

<http://example.org/ssd/assertion/9f201515da7e4a9cbe19a25b35f7a3f9> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject ssd:E1_ZNS_SSD_1 ;
    dct:source "The standardization of NVMe Zoned Namespaces (ZNS) in the NVMe 2.0 specification presents a unique new addition to storage devices." .

<http://example.org/ssd/assertion/b679047799d54d5d82d53474c002df83> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_ZNS_SSD_1 ;
    dct:source "ZNS devices push certain operations regarding data placement and garbage collection out from the device to the host." .

<http://example.org/ssd/assertion/c8d83f440d6142fa8120f42b8a3b0a46> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_ZNS_SSD_1 ;
    dct:source "Configuration of the I/O scheduler can provide workload dependent performance gains." .

<http://example.org/ssd/assertion/cc3cf17be47f4e1bbace2c57996bdfb2> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_ZNS_SSD_1 ;
    dct:source "Zoned Namespace (ZNS) SSDs are the latest addition in these efforts, which are now standardized in the NVMe 2.0 specification." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E3_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:improves a owl:ObjectProperty .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile ssd:E3_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> rdfs:label "ZNS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler> rdfs:label "I/O Scheduler" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> rdfs:label "NVMe" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

ssd:E1_ZNS_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "ZNS_SSD_1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    ssd:uses <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler> ;
    rdfs:isDefinedBy tax:SSD .

