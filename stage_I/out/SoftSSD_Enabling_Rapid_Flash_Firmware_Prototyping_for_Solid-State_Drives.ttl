ssd:axiom_06d3f00b a owl:Axiom ;
    rdfs:comment "hasPCIe rdfs:domain SSD; rdfs:range PCIe." .

ssd:axiom_370b6667 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

