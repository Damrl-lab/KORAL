<http://example.org/ssd/assertion/0fd0c802ac394f9ebcaf9c0af81d28df> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    dct:source "We show how retention relaxation can benefit ECC designs for future SSDs which require concatenated BCH-LDPC codes." .

<http://example.org/ssd/assertion/6c6e0ad80a6e468289048db1d0f6fd22> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E9_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "We observe that in typical datacenter workloads, e.g., proxy and MapReduce, many data written into storage are updated quite soon." .

<http://example.org/ssd/assertion/6d295e8f5b5346d08ed9b1c79eb343d5> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_SSD ;
    dct:source "For example, comparing recent 2-bit MLC NAND Flash memories with previous SLC ones, page write latency increased from 200 ms to 1800 ms." .

<http://example.org/ssd/assertion/76190a6377f8405e9a12b07be6ff1241> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:includes ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    dct:source "We propose an ECC architecture where data are encoded by variable ECC codes based on their retention requirements." .

<http://example.org/ssd/assertion/ae343b16395e42a0938d9e6853451878> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Simulation results show that via write speed optimization, we can achieve 1 :8–5:7× write response time speedup." .

<http://example.org/ssd/assertion/b94442b6e6354e5a94f0fe12259fb906> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E9_EnvironmentalAndOperationalContext ;
    dct:source "We observe that in typical datacenter workloads, e.g., proxy and MapReduce, many data written into storage are updated quite soon." .

<http://example.org/ssd/assertion/d35a38f4a8b848a09b35395ecc15e05c> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E6_Retention_Relaxation ;
    dct:source "Simulation results show that via write speed optimization, we can achieve 1 :8–5:7× write response time speedup." .

<http://example.org/ssd/assertion/e6de3e2590ca478cb32e6aa3ba338f3c> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E6_Retention_Relaxation ;
    dct:source "We show how retention relaxation can benefit ECC designs for future SSDs which require concatenated BCH-LDPC codes." .

ssd:hasDeviceType a owl:ObjectProperty .

