    ssd:objectLiteral "Mixed" ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E3_WP1 ;
    dct:source "We performed a thorough evaluation (5) with 9 datacenter I/O traces, 6 file system, and 15 popular data-intensive workloads." .

