    rdfs:label "Arm-based Storage Server" ;
    ssd:operatesUnder <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/Interconnects/NVMe-oF%2520%2528RoCE%252C%2520TCP%252C%2520FC%2529> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

