ssd:E3_TLC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "TLC_SSD_1" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasFirmwareVersion <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller/FirmwareVersion> ;
    ssd:hasReliability <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:hasSMARTAttribute <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:improvedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> ;
    ssd:operatesUnder ssd:E2_Enterprise_Storage_System ;
    ssd:partOf <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy> ;
    rdfs:isDefinedBy tax:SSD .

