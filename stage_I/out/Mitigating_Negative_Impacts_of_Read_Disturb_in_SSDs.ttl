<http://example.org/ssd/assertion/20ea2f8f5da94f1d9170fdfd01f07576> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E6_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E4_EC1 ;
    dct:source "Through a series of emulation tests on several realistic disk traces, we demonstrate that the proposed mechanisms can noticeably yield performance improvements on the metrics of read error rate and I/O latency." .

<http://example.org/ssd/assertion/2bf7ff7fdfe143e48e32bd2946385ec4> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "As measurements indicate, our proposal can afford a better performance improvement on the metrics of average read latency, read error rate, and I/O long-tail latency." .

<http://example.org/ssd/assertion/373c3e7c37024336adaf0e69c943b485> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "Consequently, flash density increases are then driven by Trinary-level cell (TLC) or even Quad-level cell (QLC) (3 or 4 bits/cell) combined with vertical stacking of NAND memory planes." .

<http://example.org/ssd/assertion/44f65f9ac25b4aa8a3927a28e5c8d967> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "The accumulated read operations on SSD blocks may lead to more read disturb errors, which must affect the reliability of SSDs by suffering from uncorrectable bit errors beyond the capacity of ECCs." .

<http://example.org/ssd/assertion/71c288eff19e4995ac146cb4bd5366a8> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E6_WP1 ;
    dct:source "The accumulated read operations on SSD blocks may lead to more read disturb errors, which must affect the reliability of SSDs by suffering from uncorrectable bit errors beyond the capacity of ECCs." .

<http://example.org/ssd/assertion/7e9c3921398141168f3cb3156ba39799> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "As measurements indicate, our proposal can afford a better performance improvement on the metrics of average read latency, read error rate, and I/O long-tail latency." .

<http://example.org/ssd/assertion/7fded855c8044acf88784c065a4d97b8> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "As measurements indicate, our proposal can afford a better performance improvement on the metrics of average read latency, read error rate, and I/O long-tail latency." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile ssd:E6_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E6_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:improves a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> rdfs:label "EnvironmentalAndOperationalContext" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

