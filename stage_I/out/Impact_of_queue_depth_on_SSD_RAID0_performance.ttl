<http://example.org/ssd/assertion/200a100c555e46c9af0891c569e4b67d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/Operating%2520System> ;
    ssd:predicate ssd:hasHostSystem ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Tests were carried out on Microsoft Windows 10 Education suite, with the OS on SSD on ADATA SSD devices." .

<http://example.org/ssd/assertion/36ce0195ec184023ba5c9dcb704f8227> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "The paper analyzes the effect of RAID 0 array size (number of SSD units), average block size, and queue depth to the RAID 0 array performance." .

<http://example.org/ssd/assertion/6fdc3b0bd4994a099263257b608a2727> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:hasThroughput ;
    ssd:subject ssd:E7_ADATA_SSD ;
    dct:source "Maximum reading performance of over 520 MB/s is reached with block size of 128 KB." .

<http://example.org/ssd/assertion/77a466f064704befbd9d82abc1a9e429> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "The testing is performed with workloads ranging from 512 B to 32 MB, with each consecutive workload being twice the size of the previous." .

<http://example.org/ssd/assertion/ac8fca2a358b407bb46006010b1f45e9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID0> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E7_ADATA_SSD ;
    dct:source "The RAID 0 level represents a group of N independent disks." .

<http://example.org/ssd/assertion/cb059e064cb04bf3ba12df65dc8d0f65> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E7_ADATA_SSD ;
    dct:source "Tests were carried out on Microsoft Windows 10 Education suite, with the OS on SSD on ADATA SSD devices." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E3_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasQueueDepth <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E7_ADATA_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "ADATA_SSD" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID0> ;
    ssd:hasThroughput <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID0> rdfs:label "RAID0" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID0> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/Operating%2520System> rdfs:label "Operating System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/Operating%2520System> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> rdfs:label "QueueDepth" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasHostSystem <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/Operating%2520System> ;
    ssd:hasWorkloadProfile ssd:E3_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

