@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_6a2be575 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_86daa70e a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_c6641e54 a owl:Axiom ;
    rdfs:comment "hasQueueDepth rdfs:domain EnvironmentalAndOperationalContext; rdfs:range QueueDepth." .

ssd:axiom_d01c394e a owl:Axiom ;
    rdfs:comment "hasBlockSize rdfs:domain EnvironmentalAndOperationalContext; rdfs:range BlockSize." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/179b83e546a0474c93073e225409ea74> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "Random" ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Each thread performs random 4KB read IOs." .

<http://example.org/ssd/assertion/4445a6a6c9484f7ab01143cdf19fb325> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "4KB" ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Each thread performs random 4KB read IOs." .

<http://example.org/ssd/assertion/6f684febd3ed468090b1fd9360c02212> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_FEMU ;
    dct:source "FEMU can scale to 32 IO threads and still achieve a low latency (as low as 52µs under a 2.3GHz CPU)." .

<http://example.org/ssd/assertion/74e8e5e8c9f94dc1a9039e449094b927> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_FEMU ;
    dct:source "FEMU can keep IO latency stable in less than 52 µs, and even below 90 µs at 64 IO threads." .

<http://example.org/ssd/assertion/cc39a3eb7bd8489fb80c0bb11cb21967> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_FEMU ;
    dct:source "In the experiment above, 99% of all the IOs are stable at 70 µs." .

<http://example.org/ssd/assertion/e6eea790713343d2913bb9ab44a1590b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "32" ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "FEMU can scale to 32 IO threads and still achieve a low latency." .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> rdfs:label "QueueDepth" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> .

ssd:improves a owl:ObjectProperty .

ssd:E1_FEMU a tax:SSD,
        owl:Thing ;
    rdfs:label "FEMU" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> rdfs:label "99th Percentile Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasBlockSize "4KB" ;
    ssd:hasQueueDepth "32" ;
    ssd:hasWorkloadProfile "Random" ;
    ssd:unit "AccessPattern",
        "BlockSize",
        "QueueDepth" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

