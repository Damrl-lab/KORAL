    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "SSDs have been deployed in diverse computing environments like tablets, laptops, desktops, and supercomputers." .

<http://example.org/ssd/assertion/1e1ed180cba74c64af225a733c6eb5a9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:hasEnduranceMetric ;
    ssd:subject tax:SSD ;
    dct:source "Efficient utilization of this on-board memory buffer or cache is one of the primary factors behind high performance and robustness of an SSD." .

<http://example.org/ssd/assertion/25d64a18e2a24c3c90654e92e628741e> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
