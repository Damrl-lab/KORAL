<http://example.org/ssd/assertion/a4c8af99f5134ef98d91f9a21c13f678> a ssd:Assertion ;
    ssd:confidence 0.6 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
