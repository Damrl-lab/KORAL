<http://example.org/ssd/assertion/1687811f4867430497f4e0175c4f37c9> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E4_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "Currently, SSDs are widely used in cloud storage systems and large data centers due to their high performance." .

<http://example.org/ssd/assertion/4532619c014b4df5aed9eda16b1cefbc> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E6_FDR ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "The results show that the FDR and FAR of our method outperform the baselines by 17.25% and 2.39% on average." .

<http://example.org/ssd/assertion/5f8a542330614f448214a49b9eaae433> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:predicate ssd:monitoredBy ;
    ssd:subject tax:SSD ;
    dct:source "Modern storage devices (HDDs and SSDs) have implemented the S.M.A.R.T (Self-Monitoring, Analysis and Report Technology) monitoring system, which can detect and record attributes related to drive dependability." .

<http://example.org/ssd/assertion/cc7fe8de02a14955bb304934d5f08eec> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E7_FAR ;
    ssd:predicate ssd:degrades ;
    ssd:subject tax:SSD ;
    dct:source "The results show that the FDR and FAR of our method outperform the baselines by 17.25% and 2.39% on average." .

<http://example.org/ssd/assertion/e8c938de649e4789bee3892ea6a50a34> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Currently, SSDs are widely used in cloud storage systems and large data centers due to their high performance." .

<http://example.org/ssd/assertion/ea62c0f52ff546ba8de7c9764f4f0504> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "Currently, SSDs are widely used in cloud storage systems and large data centers due to their high performance." .

ssd:degrades a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E6_FDR a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        owl:Thing ;
    rdfs:label "FDR" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

ssd:E7_FAR a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        owl:Thing ;
    rdfs:label "FAR" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> rdfs:label "SMARTAttribute" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E4_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

tax:SSD rdfs:label "SSD" ;
    ssd:degrades ssd:E7_FAR ;
    ssd:improves ssd:E6_FDR ;
    ssd:monitoredBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    rdfs:isDefinedBy tax:SSD .

