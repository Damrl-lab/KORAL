ssd:axiom_26040298 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range Performance." .

ssd:axiom_278bd18d a owl:Axiom ;
    rdfs:comment "degrades rdfs:domain SSD; rdfs:range Performance." .

ssd:axiom_6dbb4bf7 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_aadb4a41 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_aca81ba1 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_f6af7137 a owl:Axiom ;
    rdfs:comment "monitoredBy rdfs:domain SSD; rdfs:range SMARTAttribute." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

