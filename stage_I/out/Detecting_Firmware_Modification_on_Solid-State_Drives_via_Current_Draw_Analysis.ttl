@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_623bff3f a owl:Axiom ;
    rdfs:comment "hasFirmwareVersion rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic." .

ssd:axiom_77d8e32c a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:axiom_ba9dffa7 a owl:Axiom ;
    rdfs:comment "hasMaintenance rdfs:domain SSD; rdfs:range Maintenance." .

ssd:axiom_be3a0912 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_d1c0e05c a owl:Axiom ;
    rdfs:comment "hasInterfaceType rdfs:domain SSD; rdfs:range InterfaceType." .

ssd:axiom_dcb1a816 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/15d9915f41c24ae0ac561dfa18d5a8c3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E8_Greedy_50_ ;
    ssd:predicate ssd:hasFirmwareVersion ;
    ssd:subject tax:SSD ;
    dct:source "The set of buffer erasure probabilities examined includes 0%, 12.5%, 25%, 37.5%, 50%, 75%, and 100%." .

<http://example.org/ssd/assertion/167ef4281e0e4047918ebac00f9c554c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject tax:SSD ;
    dct:source "For communication with the host, the SSD can utilize the traditional Serial Advanced Technology Attachment (SATA) connector." .

<http://example.org/ssd/assertion/1b863c432e1a46b29b249511f502d499> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> ;
    ssd:predicate ssd:hasMaintenance ;
    ssd:subject tax:SSD ;
    dct:source "One of the most important tasks in the risk mitigation process is to deploy up-to-date, authenticated firmware that patches known vulnerabilities." .

<http://example.org/ssd/assertion/a5c15f7485cc48b78de581cfff9883cf> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "Our analysis is designed to be performed on disks that are in an offline mode." .

<http://example.org/ssd/assertion/d4fbd552b3fa434c9d9b348a834fd70c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "25" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "We train our classifier to ensure our ability to detect changes at a range of common operating temperatures between 25C and 50C." .

<http://example.org/ssd/assertion/e01a1e7d9ba74bf38ec5372d569db331> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    ssd:predicate ssd:hasMaintenance ;
    ssd:subject tax:SSD ;
    dct:source "Current draw measurements have also been combined with both time and frequency analysis to classify read versus write operations and infer the presence of TRIM-initiated physical data removal." .

<http://example.org/ssd/assertion/f3fd9fd9778f40ab8abbc152db6d2ef1> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "We further test this method for robustness against changes in external factors such as temperature and supplied power." .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

ssd:E4_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E8_Greedy_50_ a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "Greedy-50%" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> rdfs:label "Firmware Update" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> rdfs:label "TRIM" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> rdfs:label "SATA" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasTemperature "25" ;
    ssd:hasWorkloadProfile ssd:E4_WP1 ;
    ssd:unit "C" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

tax:SSD rdfs:label "SSD" ;
    ssd:hasFirmwareVersion ssd:E8_Greedy_50_ ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> ;
    ssd:hasMaintenance <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update>,
        <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    rdfs:isDefinedBy tax:SSD .

