<http://example.org/ssd/assertion/41c269e3484c41c5b482596497d7285c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:objectLiteral "4" ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E6_BGA_Caching_Model ;
    dct:source "SRAM caches typically consist of 64 B “lines”, DRAM of 2-4 KB “rows”, and flash/disk of 4 KB “pages”." .

<http://example.org/ssd/assertion/ad8b552248b24eaba3e9cbd296746788> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_Item_Block_Layered_Partitioning ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E6_BGA_Caching_Model ;
    dct:source "We design and analyze Item-Block Layered Partitioning, a practical BGA caching policy, and we prove an upper bound that is much tighter than any policy that considers only a single granularity." .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> rdfs:label "EnvironmentalAndOperationalContext" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> rdfs:label "WorkloadProfile" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> rdfs:label "BlockSize" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> .

ssd:E6_BGA_Caching_Model a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies>,
        owl:Thing ;
    rdfs:label "BGA Caching Model" ;
    ssd:hasBlockSize "4" ;
    ssd:improves ssd:E7_Item_Block_Layered_Partitioning ;
    ssd:unit "KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies> .

ssd:E7_Item_Block_Layered_Partitioning a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies>,
        owl:Thing ;
    rdfs:label "Item-Block Layered Partitioning" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies> rdfs:label "CachingPolicies" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies> .

