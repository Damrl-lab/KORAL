ssd:axiom_3043c213 a owl:Axiom ;
    rdfs:comment "uses rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic/DataPath/Error Correction (ECC)." .

ssd:axiom_38a70515 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

