ssd:axiom_17379964 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain InternalFirmwareAndLogic/Maintenance; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_64f66ca1 a owl:Axiom ;
    rdfs:comment "correlatesWith rdfs:domain MetricsHealthAndState/Performance; rdfs:range ConfigurationAndAdvancedParadigms/Security." .

ssd:axiom_be1d9f9e a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_f563f8fb a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

