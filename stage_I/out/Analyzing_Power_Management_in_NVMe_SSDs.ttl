ssd:E1_NVMe_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "NVMe_SSD_1" ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/PCIe> ;
    ssd:hasPowerManagement <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/PowerManagement> ;
    ssd:hasThermalManagement <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/ThermalManagement> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    rdfs:isDefinedBy tax:SSD .

