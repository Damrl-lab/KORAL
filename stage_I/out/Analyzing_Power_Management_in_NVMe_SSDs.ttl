<http://example.org/ssd/assertion/2bd52e5c030a482e8d3a06346af2c295> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_NVMe_SSD_1 ;
    dct:source "The throughput and latency of an NVMe SSD in different power consumption modes is analysed." .

<http://example.org/ssd/assertion/461e2032d65c4e5597d46fdb90af0ab0> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_NVMe_SSD_1 ;
