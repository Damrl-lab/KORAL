<http://example.org/ssd/assertion/2712e4af9b02470e8930fd8f549948cf> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We applied the widely used FIO (flexible I/O tester) tool to generate heavy I/O workloads and collect the IOPS and tail latency results." .

<http://example.org/ssd/assertion/676b271ea4ce46699858d5dca5809013> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> ;
    ssd:predicate ssd:hasRAIDLevel ;
    ssd:subject ssd:E11_ElasticRAID ;
    dct:source "We implemented a Linux software-based elastic RAID prototype that supports dynamic conversion between RAID 5 and RAID 10." .

<http://example.org/ssd/assertion/68397f06751a492eae516e976837a892> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_QD128 ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "To ensure sufficient I/O workload stress over all three RAID 5 systems, we configured FIO to run with 16 jobs at the I/O queue depth of 128." .

<http://example.org/ssd/assertion/a115751fb238456781bd7b8578466781> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "We focused on 4KB random write workloads over the entire 100% LBA span to trigger the worst-case scenarios for RAID 5." .

<http://example.org/ssd/assertion/a3f6bb64f448451589a6276902ffe2dd> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "We focused on 4KB random write workloads over the entire 100% LBA span to trigger the worst-case scenarios for RAID 5." .

<http://example.org/ssd/assertion/a65ed05dfec743e1adf9f37cdd271a33> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E11_ElasticRAID ;
    dct:source "The elastic RAID could improve the 4KB random write IOPS (I/O per second) by 42% and 4KB random read IOPS in degraded mode by 46%, while maintaining the same effective storage capacity." .

<http://example.org/ssd/assertion/a89566bed11c4cffa649d62789f2b9cb> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID10> ;
    ssd:predicate ssd:hasRAIDLevel ;
    ssd:subject ssd:E11_ElasticRAID ;
    dct:source "We implemented a Linux software-based elastic RAID prototype that supports dynamic conversion between RAID 5 and RAID 10." .

<http://example.org/ssd/assertion/c1755d85c66f43f28804ce6e34cddf4a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E11_ElasticRAID ;
    dct:source "We implemented a Linux software-based elastic RAID prototype that supports dynamic conversion between RAID 5 and RAID 10." .

<http://example.org/ssd/assertion/fb1c515444224a0882ec9293ca18119a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "We focused on 4KB random write workloads over the entire 100% LBA span to trigger the worst-case scenarios for RAID 5." .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E7_QD128 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth>,
        owl:Thing ;
    rdfs:label "QD128" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasWorkloadProfile ssd:E3_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID10> rdfs:label "RAID10" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID10> .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> rdfs:label "RAID5" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> rdfs:label "Write-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

ssd:E11_ElasticRAID a tax:SSD,
        owl:Thing ;
    rdfs:label "ElasticRAID" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:hasRAIDLevel <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID10>,
        <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

ssd:E3_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasAccessPattern <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:hasBlockSize <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:hasQueueDepth ssd:E7_QD128 ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

