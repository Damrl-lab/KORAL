<http://example.org/ssd/assertion/25aa2993724f4528b2b7eacd80ea7287> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E20_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E19_EC1 ;
    dct:source "Performance evaluations using server traces indicate that the proposed policy extends the lifespan of SSDs by up to 1.72 times compared to a policy solely performing static wear leveling." .

<http://example.org/ssd/assertion/3ab63afcc66f4debbb689a9b3f60f03a> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E19_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E6_Greedy_MP ;
