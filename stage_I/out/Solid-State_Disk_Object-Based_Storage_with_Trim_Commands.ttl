@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_3e92a1a5 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_42703609 a owl:Axiom ;
    rdfs:comment "hasAccessPattern rdfs:domain WorkloadProfile; rdfs:range AccessPattern." .

ssd:axiom_5cd33472 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic/MediaManagement/Over-Provisioning." .

ssd:axiom_6997d3f8 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_8f31d9f5 a owl:Axiom ;
    rdfs:comment "uses rdfs:domain SSD; rdfs:range ConfigurationAndAdvancedParadigms/CapacityAndProvisioning." .

ssd:axiom_e9f27e32 a owl:Axiom ;
    rdfs:comment "uses rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic/Maintenance/TRIM." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/4fd667a5b52c4378a835ffa63d3002d2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E8_WP1 ;
    dct:source "We present a model for computing utilization and write amplification under a modified random workload including Write and Trim commands." .

<http://example.org/ssd/assertion/5f03115a035b483c80f7aee56f6802b9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/CapacityAndProvisioning> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E6_SSD_1 ;
    dct:source "Object based storage offers multiple advantages for systems using solid state storage devices." .

<http://example.org/ssd/assertion/7886991464464ccf91141a7de1707d02> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E6_SSD_1 ;
    dct:source "NAND flash solid state drives (SSDs) have become ubiquitous, appearing in small mobile electronics to large data centers." .

<http://example.org/ssd/assertion/7e87b4f8bb924b7b80aba90f3d688b9e> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E6_SSD_1 ;
    dct:source "Object based storage offers multiple advantages for systems using solid state storage devices, leading to a lower write amplification and faster performance." .

<http://example.org/ssd/assertion/cd4fd4f59b494cb090c4cc0c6c135ac4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E8_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E7_EC1 ;
    dct:source "We present a model for computing utilization and write amplification under a modified random workload including Write and Trim commands." .

<http://example.org/ssd/assertion/e126e0ac2e0847e0ba725fb4f5638957> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E6_SSD_1 ;
    dct:source "The Trim command is a way for the file system to communicate to the SSD that certain data pages no longer need to be retained." .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E7_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile ssd:E8_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E8_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    ssd:hasAccessPattern <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/CapacityAndProvisioning> rdfs:label "Object-Based Storage" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/CapacityAndProvisioning> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> rdfs:label "Random Workload" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> rdfs:label "TRIM" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> rdfs:label "Write Amplification" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> .

ssd:E6_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD_1" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Over-Provisioning> ;
    ssd:operatesUnder ssd:E7_EC1 ;
    ssd:uses <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/CapacityAndProvisioning>,
        <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/TRIM> ;
    rdfs:isDefinedBy tax:SSD .

