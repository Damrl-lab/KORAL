<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> rdfs:label "SATA" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> .

ssd:E1_Smart_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "Smart SSD" ;
    ssd:communicatesWith ssd:E7_Host_System ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:implements <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ComputationalStorage/In-Drive%2520Processing> ;
    ssd:improves ssd:E5_Energy_Consumption ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:usesInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> ;
    rdfs:isDefinedBy tax:SSD .

