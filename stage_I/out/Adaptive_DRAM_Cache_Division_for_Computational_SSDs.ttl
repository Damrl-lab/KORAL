<http://example.org/ssd/assertion/77cd858c9b2e485cbd07e29ed003124e> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_Adaptive_Cache_Division ;
    dct:source "Trace-driven simulation experiments show that our proposal can reduce the overall I/O latency by 27.5% on average, in contrast to existing cache management schemes." .

<http://example.org/ssd/assertion/9987cbfa4f3341a7996544fb9e426cf6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/CacheAndBuffers/DRAM%2520Cache> ;
    ssd:predicate ssd:hasCacheAndBuffers ;
    ssd:subject ssd:E1_CompSSD ;
    dct:source "The SSD device consists of an on-board DRAM buffer, and a portion of this memory buffer is used as the data cache of SSD devices." .

<http://example.org/ssd/assertion/a2165d4fc76c4ffc8021707320716008> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/PCIe/Gen3> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject ssd:E1_CompSSD ;
    dct:source "SSD Host Interface PCIe-3.0 (NVMe-1.2)" .

<http://example.org/ssd/assertion/a518ce68f68249a2a8476eceed9e9743> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E6_WorkloadProfile ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_CompSSD ;
    dct:source "We select eight block I/O traces of real applications from the Microsoft Research Cambridge collection [24] and the VDI trace collection [25], for carrying out trace-driven experiments." .

<http://example.org/ssd/assertion/e4c9584a79f04391b91a479d8b888e24> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_Adaptive_Cache_Division ;
    dct:source "As our measurements indicate, the proposed cache division scheme can improve the overall I/O latency by between 0.9% and 84.5 %, in contrast to existing cache management schemes." .

<http://example.org/ssd/assertion/f231e30406e2476abe3b315fb8fd76f5> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_CompSSD ;
    dct:source "The DRAM cache size is typically 0.1% of the SSD capacity [23], indicating 128MB cache in our configuration." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E3_Adaptive_Cache_Division a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies>,
        owl:Thing ;
    rdfs:label "Adaptive Cache Division" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies> .

ssd:E6_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "I/O Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Overall I/O Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/CacheAndBuffers/DRAM%2520Cache> rdfs:label "DRAM Cache" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/CacheAndBuffers/DRAM%2520Cache> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> rdfs:label "QLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/PCIe/Gen3> rdfs:label "PCIe-3.0" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/PCIe/Gen3> .

ssd:E1_CompSSD a tax:SSD,
        owl:Thing ;
    rdfs:label "CompSSD" ;
    ssd:hasCacheAndBuffers <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/CacheAndBuffers/DRAM%2520Cache> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> ;
    ssd:hasInterfaceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/PCIe/Gen3> ;
    ssd:operatesUnder ssd:E6_WorkloadProfile ;
    rdfs:isDefinedBy tax:SSD .

