@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_108dd6e8 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_3110b416 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType." .

ssd:axiom_656cee68 a owl:Axiom ;
    rdfs:comment "controls rdfs:domain SSD/PhysicalHardwareAndArchitecture/Controller; rdfs:range SSD." .

ssd:axiom_87b676bf a owl:Axiom ;
    rdfs:comment "compliesWith rdfs:domain SSD; rdfs:range SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces." .

ssd:axiom_90f81c24 a owl:Axiom ;
    rdfs:comment "usesInterface rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/InterfaceType." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/0dd71544e55b43e59cb7bea8e51c5265> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_Open_Channel_SSD ;
    dct:source "With QLC, or even TLC, this management is complex, error-prone and might introduce weird constraints on the sequences of reads and writes." .

<http://example.org/ssd/assertion/12bb78f3e02b45c2ac21bacff29a6f6a> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> ;
    ssd:predicate ssd:compliesWith ;
    ssd:subject ssd:E1_Open_Channel_SSD ;
    dct:source "ZNS can be implemented as an application-specific Flash Translation Layer on top of Open-Channel SSDs." .

<http://example.org/ssd/assertion/411a5cd310fe428fb4a6110db2b537d4> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_Open_Channel_SSD ;
    dct:source "On a dual-plane TLC drive, the size of a RocksDB block must be a multiple of 96KB." .

<http://example.org/ssd/assertion/596c1425e43749d0a344bac090e7efa5> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E1_Open_Channel_SSD ;
    ssd:predicate ssd:controls ;
    ssd:subject ssd:E3_OX_Controller ;
    dct:source "We have worked with two generations of Open-Channel SSDs in the context of our work on the OX Controller." .

<http://example.org/ssd/assertion/7ce8fc7271c5473b867c3ffee7672f0d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:predicate ssd:usesInterface ;
    ssd:subject ssd:E1_Open_Channel_SSD ;
    dct:source "The Open-Channel Interface specification, defined in the context of LightNVM, is an extension of the NVMe protocol." .

<http://example.org/ssd/assertion/9286cd733ced48ba9de9ea86f2267b96> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_Open_Channel_SSD ;
    dct:source "Open-Channel SSDs have gained acceptance in data centers (e.g., Alibaba) and for computational storage (e.g., Pliops)." .

ssd:E3_OX_Controller a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller>,
        owl:Thing ;
    rdfs:label "OX Controller" ;
    ssd:controls ssd:E1_Open_Channel_SSD ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller> .

ssd:operatesUnder a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> rdfs:label "Zoned Namespaces (ZNS)" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> rdfs:label "QLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> rdfs:label "NVMe" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

ssd:E1_Open_Channel_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "Open-Channel SSD" ;
    ssd:compliesWith <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces/ZNS> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/QLC>,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:operatesUnder <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:usesInterface <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    rdfs:isDefinedBy tax:SSD .

