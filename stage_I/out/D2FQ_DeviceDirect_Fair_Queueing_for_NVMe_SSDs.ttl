<http://example.org/ssd/assertion/75bc87e228de44fda8448640508d3e14> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E9_Linux_Kernel ;
    ssd:predicate ssd:implementedIn ;
    ssd:subject ssd:E3_D2FQ ;
    dct:source "D2FQ is implemented in the Linux kernel and evaluated with various workloads." .

<http://example.org/ssd/assertion/85782b1ce57d4ef19f411225f83aeb64> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E3_D2FQ ;
    ssd:predicate ssd:usedFor ;
    ssd:subject ssd:E8_FIO ;
    dct:source "Using the FIO benchmark with various workload configurations, our scheme provides fairness while reducing CPU utilization by up to 45% as compared to MQFQ." .

<http://example.org/ssd/assertion/96df3ac3bf3c49c5bf0dbc14b06c5a5a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_NVMe_SSD ;
    dct:source "Modern high-performance SSDs can accommodate multiple independent I/O flows in multi-tenant computing environments such as cloud data centers." .

<http://example.org/ssd/assertion/9bab98a0772644e1b2e62853493e4829> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_D2FQ ;
    dct:source "The queue selection policy is carefully designed to provide fairness while reducing tail latency as much as possible." .

<http://example.org/ssd/assertion/a67c5827a6a048c0982275c89b1fb87b> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_D2FQ ;
    dct:source "D2FQ provides fairness while saving CPU cycles by up to 45% as compared to MQFQ, a state-of-the-art fair queueing I/O scheduler." .

<http://example.org/ssd/assertion/cc36d19050474003ae3753b38a07cd46> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E10_Weighted_Round_Robin__WRR_ ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E3_D2FQ ;
    dct:source "D2FQ leverages the NVMe WRR feature but does not use it as it is." .

<http://example.org/ssd/assertion/e16b4e511c694b8f9f240e231e7c9c86> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_D2FQ ;
    dct:source "D2FQ outperforms other schedulers in terms of I/O latency, CPU utilization, and reaches the maximum storage bandwidth faster than the other I/O schedulers." .

<http://example.org/ssd/assertion/edfb14269040426f98c9bdf431d0935a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_D2FQ ;
    dct:source "D2FQ reduces the I/O latency by up to 35% and improving the I/O bandwidth by up to 54%." .

ssd:E1_NVMe_SSD a tax:SSD,
