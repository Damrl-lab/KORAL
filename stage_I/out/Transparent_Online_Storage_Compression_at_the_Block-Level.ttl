<http://example.org/ssd/assertion/2df3988c402747e886e720af13a69ae8> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "We evaluate our approach in the Linux kernel on a commodity server with multicore CPUs, using Post-Mark, SPECsfs2008, TPC-C, and TPC-H." .

<http://example.org/ssd/assertion/353c59fb42ec4bbeb3e47b318c93fbe0> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Preliminary results show that transparent online block-level compression is a viable option for improving effective storage capacity, it can improve I/O performance up to 80% by reducing I/O traffic and seek distance." .

<http://example.org/ssd/assertion/5d43b9ec679342169608b24fe28e9630> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "PostMark is a file-system benchmark that simulates a mail server that uses the maildir file organization." .

<http://example.org/ssd/assertion/61dfc7fa00c04e609c78dc42382c4898> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We evaluate our approach in the Linux kernel on a commodity server with multicore CPUs, using Post-Mark, SPECsfs2008, TPC-C, and TPC-H." .

<http://example.org/ssd/assertion/7edda6856b5546bab75eb14616185eb4> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
