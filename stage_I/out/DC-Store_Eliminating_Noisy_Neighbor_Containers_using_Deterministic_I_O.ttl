<http://example.org/ssd/assertion/02e99ad9fad64d52ba0a57de938364e8> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_Divided_SSD ;
    ssd:predicate ssd:hasComponent ;
    ssd:subject ssd:E1_DC_Store ;
    dct:source "We design and implement Divided SSD, which separates physical flash channels and internal resources to support multiple NVM sets." .

<http://example.org/ssd/assertion/0ea74254d6844a32a8362465189c6080> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E10_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E9_EnvironmentalAndOperationalContext ;
    dct:source "We execute a Facebook-like workload (interspersing bursty writes into sustained reads) as a representative I/O access pattern of cloud environment." .

<http://example.org/ssd/assertion/0fca3ec2af8c40d2999fbeda5ae7b1d6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E4_Divided_SSD ;
    dct:source "The proposed Divided SSD can provide an independent environment, which allows multiple containers to operate without the device-level I/O interference." .

<http://example.org/ssd/assertion/11e4b2800fce4314801eefa60ad6d7d3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_I_O_Tacker ;
    ssd:predicate ssd:hasComponent ;
    ssd:subject ssd:E1_DC_Store ;
    dct:source "The proposed I/O Tacker modifies Linux kernel in order to enable per-container page frame reclaiming by considering the container ownership and exposed NVM set organization." .

<http://example.org/ssd/assertion/24977e562aa44f6aa638e021d76f1915> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_DC_Store ;
    dct:source "The evaluation results demonstrate that containerized data-intensive applications on DC-Store exhibit 31% shorter average execution time, on average, compared to those on a baseline system." .

<http://example.org/ssd/assertion/2d09c7c9c890484e846d7444ffa105bf> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E9_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_DC_Store ;
    dct:source "We propose DC-store, a storage framework that offers deterministic I/O performance for a multi-container execution environment." .

<http://example.org/ssd/assertion/441ee6b124c642be87912b0d56e4b67d> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E9_EnvironmentalAndOperationalContext ;
    dct:source "Docker container technology is applied to diverse computing domains such as datacenter, serverless, cloud, and high-performance computing." .

<http://example.org/ssd/assertion/70546621deaf411dbea801feb92bc74f> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E4_Divided_SSD ;
    dct:source "Our results show that, even though the average latency of our Divided SSD prototype is worse than a baseline NVMe SSD for a single volume accesses (with a specific pattern), it provides better and deterministic I/O performance under concurrent storage accesses." .

<http://example.org/ssd/assertion/e61c722379714f4f8c97a24d0a82641d> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E4_Divided_SSD ;
    dct:source "In total, our prototype hardware employs 64 flash dies, each using 64 layered triple-level cell (TLC) flash technology." .

ssd:degrades a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E10_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E5_I_O_Tacker a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "I/O Tacker" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> rdfs:label "99th Percentile Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

ssd:E1_DC_Store a tax:SSD,
        owl:Thing ;
    rdfs:label "DC-Store" ;
    ssd:hasComponent ssd:E4_Divided_SSD,
        ssd:E5_I_O_Tacker ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:operatesUnder ssd:E9_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

ssd:E9_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E10_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E4_Divided_SSD a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture>,
        owl:Thing ;
    rdfs:label "Divided SSD" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture> .

