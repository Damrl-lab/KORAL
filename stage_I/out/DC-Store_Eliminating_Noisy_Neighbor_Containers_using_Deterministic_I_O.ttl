ssd:axiom_50ea7b91 a owl:Axiom ;
    rdfs:comment "hasComponent rdfs:domain SSD; rdfs:range PhysicalHardwareAndArchitecture." .

ssd:axiom_66fe869d a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_6c18f655 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_6c7e83f6 a owl:Axiom ;
    rdfs:comment "degrades rdfs:domain PhysicalHardwareAndArchitecture; rdfs:range MetricsHealthAndState/Performance/Average Latency." .

ssd:axiom_9b52bd72 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_9c512267 a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

