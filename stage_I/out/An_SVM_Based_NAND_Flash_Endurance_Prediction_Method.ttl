ssd:axiom_24097f68 a owl:Axiom ;
    rdfs:comment "hasAccessPattern rdfs:domain WorkloadProfile; rdfs:range AccessPattern." .

ssd:axiom_24d81293 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_58e0d659 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range FlashTechnology/CellType." .

ssd:axiom_5fcedb87 a owl:Axiom ;
    rdfs:comment "correlatesWith rdfs:domain Reliability; rdfs:range Endurance." .

ssd:axiom_abf83a2a a owl:Axiom ;
    rdfs:comment "hasStructure rdfs:domain SSD; rdfs:range FlashTechnology/Structure." .

ssd:axiom_e3ed1bae a owl:Axiom ;
    rdfs:comment "improves rdfs:domain DataPath/Error Correction (ECC); rdfs:range Endurance." .

ssd:axiom_fffc665f a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/0b5b466e4e464ff9b5ee0eb5ecfaafac> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject ssd:E9_Raw_Bit_Error ;
    dct:source "The RBE number is the number of bits of difference between the actual read data and the actual programmed data without error correction, which is an important parameter to characterize the degree of endurance change." .

<http://example.org/ssd/assertion/1bad7fdb326b45f0ba0ed78987e36e52> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:hasEndurance ;
    ssd:subject ssd:E2_NAND_Flash_Memory ;
    dct:source "Using machine learning algorithms to accurately predict endurance levels can optimize wear-leveling strategies and warn bad memory blocks." .

<http://example.org/ssd/assertion/20524d03235b49c483936be1ebf36560> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E2_NAND_Flash_Memory ;
    dct:source "The flash memory particle type of this model is 3D TLC." .

<http://example.org/ssd/assertion/83980e3ce72d4ac1bec4bb56246ceb0d> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E6_WorkloadProfile ;
    dct:source "The random programming pattern can simulate various programming levels and combinations, fitting the programming pressure in practical applications to the greatest extent, which sets it as the default programming pattern in the proposed prediction method." .

<http://example.org/ssd/assertion/92405829b77548b08da4852ba79b6f8b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    dct:source "The Error Correcting Code (ECC) error correction algorithm uses special coding rules to check and correct the original data, so the endurance of the flash memory can be indirectly improved by optimizing the error correction algorithm." .

<http://example.org/ssd/assertion/abc6ae746a64475ea9380bbe1a214d45> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasStructure ;
    ssd:subject ssd:E2_NAND_Flash_Memory ;
    dct:source "The flash memory particle type of this model is 3D TLC." .

<http://example.org/ssd/assertion/b95cd54b40014cae836ae4a468b5b014> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E5_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_NAND_Flash_Memory ;
    dct:source "The random programming pattern can simulate various programming levels and combinations, fitting the programming pressure in practical applications to the greatest extent, which sets it as the default programming pattern in the proposed prediction method." .

<http://example.org/ssd/assertion/e8ff148ce36f498583eeba11c9cf89dc> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E6_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E5_EnvironmentalAndOperationalContext ;
    dct:source "The random programming pattern can simulate various programming levels and combinations, fitting the programming pressure in practical applications to the greatest extent, which sets it as the default programming pattern in the proposed prediction method." .

ssd:E9_Raw_Bit_Error a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability>,
        owl:Thing ;
    rdfs:label "Raw Bit Error" ;
    ssd:correlatesWith <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

ssd:correlatesWith a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> rdfs:label "Error Correction (ECC)" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

ssd:E5_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasWorkloadProfile ssd:E6_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E6_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasAccessPattern <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> rdfs:label "3D" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> .

ssd:E2_NAND_Flash_Memory a tax:SSD,
        owl:Thing ;
    rdfs:label "NAND Flash Memory" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:hasEndurance <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:hasStructure <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:operatesUnder ssd:E5_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

