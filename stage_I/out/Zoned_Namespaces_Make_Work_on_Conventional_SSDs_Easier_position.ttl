    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range ConfigurationAndAdvancedParadigms/ZonedNamespaces." .

ssd:axiom_e2b24538 a owl:Axiom ;
    rdfs:comment "reduces rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic/MediaManagement/Garbage Collection." .

