@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_66ad78c0 a owl:Axiom ;
    rdfs:comment "hasDeviceType rdfs:domain SSD; rdfs:range SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType." .

ssd:axiom_987cfeb0 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_deeffe9d a owl:Axiom ;
    rdfs:comment "improvedBy rdfs:domain SSD; rdfs:range SSD/InternalFirmwareAndLogic/Maintenance/Firmware Update." .

ssd:axiom_ecfd7d1a a owl:Axiom ;
    rdfs:comment "correlatesWith rdfs:domain SSD; rdfs:range SSD/MetricsHealthAndState/Reliability." .

ssd:axiom_f040384f a owl:Axiom ;
    rdfs:comment "hasReliabilityMetric rdfs:domain SSD; rdfs:range SSD/MetricsHealthAndState/Reliability." .

ssd:confidence a owl:DatatypeProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/210cbc41e2e740988e3b54e202ada5be> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> ;
    ssd:predicate ssd:improvedBy ;
    ssd:subject tax:SSD ;
    dct:source "Our observations emphasize the importance of firmware updates, as earlier firmware versions can be correlated with significantly higher failure rates." .

<http://example.org/ssd/assertion/22fc844c1848484793120251e53c3e16> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_Enterprise_Storage_System ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "This paper presents the first large-scale field study of NAND-based SSDs in enterprise storage systems." .

<http://example.org/ssd/assertion/58083d3f00f5433c90d8a766fc934b2f> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject tax:SSD ;
    dct:source "The RAID subsystem can be configured to use SSDs in a Raid-TEC (triple Parity), RAID-DP (double Parity), or RAID-4 (single Parity) configuration." .

<http://example.org/ssd/assertion/9ad3dd30c289485298debd4d2f26142f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:hasReliabilityMetric ;
    ssd:subject tax:SSD ;
    dct:source "The average ARR across the entire population is 0.22%, but rates vary widely depending on the drive model." .

<http://example.org/ssd/assertion/9de4ca2d425d4b5e8a201be0ce2ada9c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject tax:SSD ;
    dct:source "The drives comprise three different manufacturers, 18 different models, 12 different capacities, and all major flash technologies (SLC, cMLC, eMLC, 3D-TLC)." .

<http://example.org/ssd/assertion/d1e986b4df4041f0b1f84b6befcdc08b> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject tax:SSD ;
    dct:source "One third of all drive replacements are merely preventative (category D) using predictions of future drive failures." .

<http://example.org/ssd/assertion/fe0b39ba8d8f4723ab58f31f4a182895> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject tax:SSD ;
    dct:source "The drives comprise three different manufacturers, 18 different models, 12 different capacities, and all major flash technologies (SLC, cMLC, eMLC, 3D-TLC)." .

ssd:improvedBy a owl:DatatypeProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller/FirmwareVersion> rdfs:label "Firmware Version" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller/FirmwareVersion> .

ssd:E2_Enterprise_Storage_System a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment>,
        owl:Thing ;
    rdfs:label "Enterprise Storage System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment> .

ssd:correlatesWith a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy> rdfs:label "RAID" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> rdfs:label "Firmware Update" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> rdfs:label "TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> rdfs:label "3D-TLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> rdfs:label "ARR",
        "Predictive Failures",
        "Reliability" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

tax:SSD rdfs:label "SSD" ;
    ssd:correlatesWith <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC>,
        <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:hasReliabilityMetric <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:improvedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/Maintenance/Firmware%2520Update> ;
    ssd:operatesUnder ssd:E2_Enterprise_Storage_System ;
    rdfs:isDefinedBy tax:SSD .

