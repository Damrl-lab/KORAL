@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_011e8beb a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic/CachingPolicies; rdfs:range SSD/MetricsHealthAndState/Performance." .

ssd:axiom_64d5b281 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_7f6095f8 a owl:Axiom ;
    rdfs:comment "decreases rdfs:domain SSD/InternalFirmwareAndLogic/CachingPolicies; rdfs:range SSD/MetricsHealthAndState/Endurance." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/2a2a59b270714c6e848c38d6981fe796> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    dct:source "Through some preliminary studies in realistic enterprise scale traces (see Section III), we find that the differences in I/O traffic and locality of the read and write requests for modern block-level I/O traces can be complementary." .

<http://example.org/ssd/assertion/3ebe82ce988946ec9a7d042e56cae8b9> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E7_WorkloadProfile ;
    dct:source "We find that burst I/O contains a large proportion of traffic, especially for read I/O." .

<http://example.org/ssd/assertion/64979a855b814647920e57dd5744e053> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_L_CoRW ;
    dct:source "Experimental results on the latest block-level I/O traces show that L-CoRW significantly reduces the response time and tail latency by up to 48.6% and 33.7%, respectively, over the previous schemes with negligible cost." .

<http://example.org/ssd/assertion/af403c9feab94f7395bd26e78f54ed18> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_Write_Amplification_Factor ;
    ssd:predicate ssd:decreases ;
    ssd:subject ssd:E2_L_CoRW ;
    dct:source "Meanwhile, L-CoRW can reduce write amplification factor (WAF) by 9.6% to 24.7%." .

<http://example.org/ssd/assertion/c55308f139394f489d93e641a7e12f1b> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E7_WorkloadProfile ;
    dct:source "In addition, some aggressive caching policies also affect the SSD related properties, like WAF." .

<http://example.org/ssd/assertion/cc374cf5da60408ba975d4997457fcf5> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_L_CoRW ;
    dct:source "Experimental results on the latest block-level I/O traces show that L-CoRW significantly reduces the response time and tail latency by up to 48.6% and 33.7%, respectively, over the previous schemes with negligible cost." .

<http://example.org/ssd/assertion/fa6ef7edb56e4763b8185307cbb05adc> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Through some preliminary studies in realistic enterprise scale traces (see Section III), we find that the differences in I/O traffic and locality of the read and write requests for modern block-level I/O traces can be complementary." .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E5_Write_Amplification_Factor a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance>,
        owl:Thing ;
    rdfs:label "Write Amplification Factor" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    ssd:operatesUnder <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    rdfs:isDefinedBy tax:SSD .

ssd:E2_L_CoRW a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies>,
        owl:Thing ;
    rdfs:label "L-CoRW" ;
    ssd:decreases ssd:E5_Write_Amplification_Factor ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/CachingPolicies> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> rdfs:label "Read-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> rdfs:label "Write-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> rdfs:label "99th Percentile Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

ssd:E7_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy>,
        <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> rdfs:label "Enterprise Server" ;
    ssd:hasWorkloadProfile ssd:E7_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> .

