@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_4a9accdc a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_6023e803 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic/MediaManagement/Garbage Collection; rdfs:range SSD/MetricsHealthAndState/Performance." .

ssd:axiom_64ab0370 a owl:Axiom ;
    rdfs:comment "correlatesWith rdfs:domain SSD/InternalFirmwareAndLogic/MediaManagement/Garbage Collection; rdfs:range SSD/MetricsHealthAndState/Performance." .

ssd:confidence a owl:DatatypeProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/1e1a08d02ba3492ab57eb1b06cd6d228> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object ssd:E8_CPU_Overhead ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E4_CCB_Policy ;
    dct:source "Our experimental results show that our proposed GC policy offers up to 74% less CPU overhead, compared to the best competing strategy." .

<http://example.org/ssd/assertion/2f9539eda7884844b6c56ad3ec5c9e54> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_Enterprise_Server ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_ZNS_SSD ;
    dct:source "We, for the first time, evaluate the overhead of GC policies for ZNS SSDs." .

<http://example.org/ssd/assertion/7ab373eb282f4f7ba18c7894688d8b22> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_ZNS_SSD ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject tax:SSD ;
    dct:source "A new type of SSDs, called Zoned Namespace (ZNS) SSDs, enables the partitioning of responsibilities between the host and the SSD through the ZNS storage interface." .

<http://example.org/ssd/assertion/adfe56e27d894952aae30640556afe98> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:hasPerformance ;
    ssd:subject tax:SSD ;
    dct:source "Vendors have been able to scale the capacity of SSDs to tens of TBs of storage at a performance above a million read IOPS." .

<http://example.org/ssd/assertion/d077ae4bf2ab412aaa409e1e93adf77d> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E6_Write_Amplification_Factor ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject ssd:E4_CCB_Policy ;
    dct:source "CCB maintains several linked-lists and places blocks, based on their number of invalid pages, in different linked-lists." .

ssd:correlatesWith a owl:ObjectProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_CCB_Policy a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection>,
        owl:Thing ;
    rdfs:label "CCB Policy" ;
    ssd:correlatesWith ssd:E6_Write_Amplification_Factor ;
    ssd:improves ssd:E8_CPU_Overhead ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

ssd:E6_Write_Amplification_Factor a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        owl:Thing ;
    rdfs:label "Write Amplification Factor" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

ssd:E7_Enterprise_Server a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server>,
        owl:Thing ;
    rdfs:label "Enterprise Server" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> .

ssd:E8_CPU_Overhead a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        owl:Thing ;
    rdfs:label "CPU Overhead" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

ssd:E2_ZNS_SSD a <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces>,
        owl:Thing ;
    rdfs:label "ZNS SSD" ;
    ssd:operatesUnder ssd:E7_Enterprise_Server ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces> .

tax:SSD rdfs:label "SSD" ;
    ssd:hasDeviceType ssd:E2_ZNS_SSD ;
    ssd:hasPerformance <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "Garbage Collection" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

