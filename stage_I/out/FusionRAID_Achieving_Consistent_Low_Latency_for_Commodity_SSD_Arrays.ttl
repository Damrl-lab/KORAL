<http://example.org/ssd/assertion/39de9ab9278540a2ae43be00816f2360> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E10_FusionRAID ;
    dct:source "FusionRAID brings a 22%–98% reduction in median latency, and a 2.7 ×–62× reduction in tail latency, with a moderate and temporary space overhead." .

<http://example.org/ssd/assertion/3be6d85e1161437e91f2cc40cc5fe944> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We then propose a new RAID architecture, FusionRAID, designed to simultaneously reduce the average- and worst-case latencies of SSD RAID, especially for latency-critical applications." .

<http://example.org/ssd/assertion/901b170a714544c9bbbead79711bb232> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:hasPerformanceMetric ;
    ssd:subject ssd:E8_SSD_RAID ;
    dct:source "Our evaluation with traces and applications shows that FusionRAID brings a 22%–98% reduction in median latency, and a 2.7 ×–62× reduction in tail latency, with a moderate and temporary space overhead." .

<http://example.org/ssd/assertion/9db67cd2c7394849bfeedd84206429e4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E8_SSD_RAID ;
    dct:source "Our results also reveal that with SSD latency low and decreasing, the software overhead of RAID write creates long, complex write paths involving more drives, raising both average-case latency and risk of exposing worst-case performance." .

<http://example.org/ssd/assertion/c4b26af2849d48ccb98c951c1f8d0661> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "By performing temporary, replicated writes, it retains RAID fault-tolerance yet greatly accelerates small, random writes." .

<http://example.org/ssd/assertion/cf919ca1384b42bcb835e52a346a7a15> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E4_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "By performing temporary, replicated writes, it retains RAID fault-tolerance yet greatly accelerates small, random writes." .

<http://example.org/ssd/assertion/e4b398c2179847cca030bada740f9ab7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/64KB> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "Figure 4: Sample spike behavior in write tests on two SSDs (write request size at 64KB)" .

<http://example.org/ssd/assertion/fbc290bcaf9d4f6ca9b32bfbb5a4999a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "Parity-based RAID is unfriendly to write-intensive workloads, especially for those dominated by random small writes." .

ssd:E10_FusionRAID a <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5>,
        owl:Thing ;
    rdfs:label "FusionRAID" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> .

