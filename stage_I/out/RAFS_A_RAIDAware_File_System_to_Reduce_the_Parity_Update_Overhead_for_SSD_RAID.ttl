@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_4a32f53c a owl:Axiom ;
    rdfs:comment "alignsWritesTo rdfs:domain SSD/InternalFirmwareAndLogic; rdfs:range SSD/InternalFirmwareAndLogic." .

ssd:axiom_6189e4e6 a owl:Axiom ;
    rdfs:comment "utilizes rdfs:domain SSD/InternalFirmwareAndLogic; rdfs:range SSD/EnvironmentalAndOperationalContext/HostSystem/File System." .

ssd:axiom_80cd2504 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD/InternalFirmwareAndLogic; rdfs:range SSD/MetricsHealthAndState/Performance." .

ssd:axiom_8ca55ac0 a owl:Axiom ;
    rdfs:comment "reduces rdfs:domain SSD/InternalFirmwareAndLogic; rdfs:range SSD/InternalFirmwareAndLogic/MediaManagement/Garbage Collection." .

ssd:axiom_f7d91090 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD/InternalFirmwareAndLogic; rdfs:range SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/22d6b3fcc3734128ad68a8e62e684a27> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:predicate ssd:utilizes ;
    ssd:subject ssd:E1_RAFS ;
    dct:source "RAFS employs a stripe-aware buffer policy to coalesce writes to the same file." .

<http://example.org/ssd/assertion/3435338c4fbd48a3a41568d39257ee9e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_RAFS ;
    dct:source "RAFS aims to reduce parity updates by leveraging delayed allocation in modern file systems." .

<http://example.org/ssd/assertion/550525c2db0348ecbb28f214e82733ea> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_RAFS ;
    dct:source "Experimental results show that RAFS can improve throughput by up to 90%, compared to Ext4." .

<http://example.org/ssd/assertion/666730cedbc04b4197235d35ba16b3e3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_SSD_RAID_5 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_RAFS ;
    dct:source "RAFS, a RAID-aware file system, which utilizes a RAID-friendly data layout to improve the reliability and performance of SSD-based RAID 5." .

<http://example.org/ssd/assertion/8aa6079c40044cdbaade8f25667d3806> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:reduces ;
    ssd:subject ssd:E1_RAFS ;
    dct:source "RAFS compacts buffered updates and flushes back in stripe units to mitigate the parity update overhead." .

<http://example.org/ssd/assertion/a7d6c96194284e8ca5bced44731ab789> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E7_Stripe ;
    ssd:predicate ssd:alignsWritesTo ;
    ssd:subject ssd:E1_RAFS ;
    dct:source "RAFS employs a stripe-granularity allocation scheme to align writes to stripe boundaries." .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E2_SSD_RAID_5 a <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5>,
        owl:Thing ;
    rdfs:label "SSD RAID 5" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5> .

ssd:E7_Stripe a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "Stripe" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> rdfs:label "File System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "Garbage Collection" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> rdfs:label "Write Endurance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:E1_RAFS a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "RAFS" ;
    ssd:alignsWritesTo ssd:E7_Stripe ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E2_SSD_RAID_5 ;
    ssd:reduces <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:utilizes <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

