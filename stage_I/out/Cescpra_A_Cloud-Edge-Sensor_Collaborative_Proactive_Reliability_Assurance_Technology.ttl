@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_ba17b80c a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_bf50713b a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_d29a0c61 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_fcaaaf3d a owl:Axiom ;
    rdfs:comment "hasTemperature rdfs:domain EnvironmentalAndOperationalContext; rdfs:range xsd:decimal." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/19bd94951cac4909bfcd422d819554c3> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "The data collected by the sensor is stored nearby in an Edge device with storage capacity, such as the temperature and humidity of the air and health status information detected by smart wearable devices." .

<http://example.org/ssd/assertion/3b9940cf2399448f8228d3ced065e09f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Edge%2520Device/IoT> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "In the Internet of Everything (IoE) era, various sensors configured at the Internet of Things (IoT) edge (Edge) collect massive amounts of data daily." .

<http://example.org/ssd/assertion/8767811c9da94488a44e30217e789de2> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E5_Hard_Drive_Failure_Prediction_Model ;
    dct:source "The multidisk joint proactive recovery method reduced fault tolerance time by up to 64.7% and 62.5% compared to individual migration or rebuild methods." .

<http://example.org/ssd/assertion/f981c5e721f0496dacf254388fd72c31> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Sensors collect data and store it at the Edge using erasure coding, with data and parity blocks stored separately on the Edge and in the cloud, periodically updated asynchronously." .

<http://example.org/ssd/assertion/fcb0e51324b94937a7468897e9565610> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E5_Hard_Drive_Failure_Prediction_Model ;
    dct:source "Using AI for hard disk failure prediction is a novel reliability assurance technology, significantly enhancing the reliability of sensor data." .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E5_Hard_Drive_Failure_Prediction_Model a <http://example.org/ssd/taxonomy/SSD/ModelingTimeAndStateHistory/DerivedIndicators>,
        owl:Thing ;
    rdfs:label "Hard Drive Failure Prediction Model" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ModelingTimeAndStateHistory/DerivedIndicators> .

ssd:improves a owl:ObjectProperty .

tax:SSD rdfs:label "SSD" ;
    ssd:operatesUnder ssd:E2_EC1 ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Edge%2520Device/IoT> rdfs:label "Edge Device/IoT" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Edge%2520Device/IoT> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Performance" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> rdfs:label "Reliability" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Edge%2520Device/IoT> ;
    ssd:hasWorkloadProfile ssd:E4_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

