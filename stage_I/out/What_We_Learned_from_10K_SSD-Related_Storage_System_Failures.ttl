<http://example.org/ssd/assertion/7858615547884693abe623077b928261> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject ssd:E8_SSD_Alibaba ;
    dct:source "Note that all five models in our dataset are using SATA interfaces and based on MLC NAND cells." .

<http://example.org/ssd/assertion/860c4060ef41415f8387e344d462e99a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E8_SSD_Alibaba ;
    dct:source "We study the reliability of SSD-based storage systems deployed in Alibaba Cloud, which cover near half a million SSDs and span over three years of usage under representative cloud services." .

<http://example.org/ssd/assertion/b12dd7ca48a340b8b04eb46e647f9eea> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/UltraDMA%2520CRC%2520Error%2520Count%2520%2520%255BID%253A%2520199%255D> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject ssd:E8_SSD_Alibaba ;
    dct:source "We discover a strong correlation between one type of RASR failures (i.e., those caused by faulty interconnection) and one type of device errors (i.e., Ultra-DMA CRC or UCRC)." .

<http://example.org/ssd/assertion/b2281fd61da34bcda243cc9f09fd9fdc> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "28" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E7_EC1 ;
    dct:source "We are able to verify that the poor rack architecture can increase the temperature of idle SSDs by up to 28 C." .

<http://example.org/ssd/assertion/b70ccbbd38a749c6bae124ce50b1b3d6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E7_EC1 ;
    dct:source "We study SSD-based large-scale storage systems deployed in 7 datacenters." .

<http://example.org/ssd/assertion/ddd8e62d824445f9994478bc4757970a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E8_SSD_Alibaba ;
    dct:source "Note that all five models in our dataset are using SATA interfaces and based on MLC NAND cells." .

ssd:correlatesWith a owl:ObjectProperty .

