@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_39eff36c a owl:Axiom ;
    rdfs:comment "hasQueueDepth rdfs:domain WorkloadProfile; rdfs:range QueueDepth." .

ssd:axiom_4160131d a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_b5dcb6b1 a owl:Axiom ;
    rdfs:comment "hasBlockSize rdfs:domain WorkloadProfile; rdfs:range BlockSize." .

ssd:axiom_c0260a50 a owl:Axiom ;
    rdfs:comment "hasReadWriteMix rdfs:domain WorkloadProfile; rdfs:range ReadWriteMix." .

ssd:axiom_c61828dc a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range Performance." .

ssd:axiom_df2afa3b a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/07210455bb5147ea83dcdeec098eaab8> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "In this experiment, we set the key-value pair size for all threads to 4KB." .

<http://example.org/ssd/assertion/10f43cba5dc64ccc8e8ce5bb28b0fa4c> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Our evaluation demonstrates geomean reduction of tail latency by 2.19× and 2.45× for db_bench and MixGraph, respectively, with a maximum reduction of 3.02× and 4.73×." .

<http://example.org/ssd/assertion/29b30e508bae40a18a1f45efbbf0ef62> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "The increasing ratio of write operations causes conflicts between the put queries, such as delaying the composition of batch-group writes." .

<http://example.org/ssd/assertion/71c55e4e446f4c28937b403cdcd57378> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E4_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E3_EnvironmentalAndOperationalContext ;
    dct:source "For evaluation we use both microbenchmarks (db_bench) with varying read/write ratios and key skewnesses, and realistic social-graph workloads (MixGraph from Facebook)." .

<http://example.org/ssd/assertion/786cc719ef0c4ca98492fd6f3ae1945a> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD4> ;
    ssd:predicate ssd:isInstanceOf ;
    ssd:subject ssd:E7_QueueDepth ;
    dct:source "The append command provides an efficient mechanism to increase the queue depth while maintaining the write order." .

<http://example.org/ssd/assertion/86a4b7efd07a493a8e6e0bae6970689b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
    dct:source "As a side effect of eliminating the overhead of batch-group writes, WALTZ also improves the query throughput (QPS) by up to 11.7%." .

<http://example.org/ssd/assertion/902299887cf544fe82538a61f3152d80> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "We evaluate WALTZ using both db_bench microbenchmarks with varying read/write ratios and key skewnesses, and Facebook MixGraph benchmarks." .

<http://example.org/ssd/assertion/b8b1fb604f6f4b7eac9c949d8aa1bff9> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E7_QueueDepth ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E4_WorkloadProfile ;
    dct:source "The append command provides an efficient mechanism to increase the queue depth while maintaining the write order." .

<http://example.org/ssd/assertion/cd65857aa10c4c308f40fcacc6482b09> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_SSD ;
    dct:source "We propose WALTZ, an LSM tree-based key-value store on the emerging Zoned Namespace (ZNS) SSD." .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:E3_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasWorkloadProfile ssd:E4_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E7_QueueDepth a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth>,
        owl:Thing ;
    rdfs:label "QueueDepth" ;
    ssd:isInstanceOf <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD4> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces> rdfs:label "ZNS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD4> rdfs:label "QD4" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD4> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> rdfs:label "Write-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> rdfs:label "99th Percentile Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:E1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ZonedNamespaces> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E3_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

ssd:E4_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasBlockSize <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:hasQueueDepth ssd:E7_QueueDepth ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

