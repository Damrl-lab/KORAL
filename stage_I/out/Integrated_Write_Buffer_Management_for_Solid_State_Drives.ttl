<http://example.org/ssd/assertion/20395e621ec847248df793f2988f27be> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E6_WP1 ;
    dct:source "Heavy random write requests generate numerous copy and erase operations, even for valid (i.e. non-updated) pages internally." .

<http://example.org/ssd/assertion/62aaa95d44374008b41c1159ba2a5392> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E2_IWM_SSD ;
    dct:source "We used MLC NAND flash memory of which characteristics are shown in Table 1." .

<http://example.org/ssd/assertion/afb1f401b0d044f4baedf7b99f5cc2f3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_IWM_SSD ;
    dct:source "The proposed scheme greatly outperforms previous schemes in terms of write amplification, block erase count, and execution time." .

<http://example.org/ssd/assertion/b77c81a7eab447509186def48d81300f> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E6_WP1 ;
    dct:source "However, random write within SSD is still far slower than sequential read/write and random read." .

<http://example.org/ssd/assertion/c0207047f80547688865496913185fef> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E6_WP1 ;
    dct:source "Page size of the write buffer is set to 4 KB in order to align with that of the flash memory." .

<http://example.org/ssd/assertion/cecf2fb1041c4136b63b2c77e46f7b22> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_IWM_SSD ;
    dct:source "The proposed scheme greatly outperforms previous schemes in terms of write amplification, block erase count, and execution time." .

<http://example.org/ssd/assertion/e966e52572bd470b959997ea075c39d8> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth/QD1> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E6_WP1 ;
    dct:source "The size of the write buffers range from 2 MB to 32 MB." .

<http://example.org/ssd/assertion/ecce02ab2ae9421891ad24be718071a6> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E6_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E4_EC1 ;
    dct:source "Extensive experiments, under a variety of experimental configurations, show that IWM, in most cases, shows a better performance than any other policy." .

<http://example.org/ssd/assertion/f02d94aefd6e4d82ab698e865d037ec8> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_IWM_SSD ;
    dct:source "IWM decreased the block erase count by up to 50%, as compared to other schemes." .

<http://example.org/ssd/assertion/fb26cfe065ef45dc8f5d7bee750dcf85> a ssd:Assertion ;
