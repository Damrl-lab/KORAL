<http://example.org/ssd/assertion/9936d7a91a65477d9204ce8559793cc8> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E6_Write_Stress ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E7_Opportunistic_In_place_Delta_Compression ;
    dct:source "The results show that the proposed design solution can largely reduce the write stress on SLC-mode flash memory pages without significant latency overhead." .

ssd:E7_Opportunistic_In_place_Delta_Compression a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement>,
        owl:Thing ;
    rdfs:label "Opportunistic In-place Delta Compression" ;
    ssd:improves ssd:E6_Write_Stress ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement> .

