@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_a25ea230 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_ba44593b a owl:Axiom ;
    rdfs:comment "impacts rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_be55f8d8 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_d93ef4a1 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_e5d8c1ad a owl:Axiom ;
    rdfs:comment "degrades rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/552536c659cb4c2d9470e1ffb0c8b3fb> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E9_SATA_SSD ;
    dct:source "Figure 8 unveils that the WA when using the SATA SSD is much smaller than that on the Optane SSD, by up to 16.59%." .

<http://example.org/ssd/assertion/6590672afc314c04b4aa80a92df5d8de> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E6_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E5_RocksDB ;
    dct:source "In this paper, we focus on the four different file-picking policies offered by RocksDB." .

<http://example.org/ssd/assertion/6f81009144e74c499511b3f4955655ec> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E8_Optane_SSD ;
    dct:source "Figure 8 unveils that the WA when using the SATA SSD is much smaller than that on the Optane SSD, by up to 16.59%." .

<http://example.org/ssd/assertion/70447d3da05d43098a1069facaae6929> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E6_EC1 ;
    dct:source "We experiment using RocksDB (v8.8.1) that supports the up-to-date RoundRobin policy and incorporates a new file splitting mechanism to reduce overlapping ratio with deeper levels." .

<http://example.org/ssd/assertion/d533cd1ea8ea418491dba044ba5fe21e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:impacts ;
    ssd:subject ssd:E5_RocksDB ;
    dct:source "Compactions in LSM-trees lead to Write Amplification (WA)." .

ssd:E8_Optane_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "Optane SSD" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    rdfs:isDefinedBy tax:SSD .

ssd:E9_SATA_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SATA SSD" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    rdfs:isDefinedBy tax:SSD .

ssd:degrades a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E5_RocksDB a tax:SSD,
        owl:Thing ;
    rdfs:label "RocksDB" ;
    ssd:impacts <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:operatesUnder ssd:E6_EC1 ;
    rdfs:isDefinedBy tax:SSD .

ssd:E7_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

ssd:E6_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasWorkloadProfile ssd:E7_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> rdfs:label "EnvironmentalAndOperationalContext" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> rdfs:label "WorkloadProfile" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Write Amplification" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

