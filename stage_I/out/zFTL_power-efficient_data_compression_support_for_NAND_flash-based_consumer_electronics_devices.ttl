<http://example.org/ssd/assertion/a02cafe335fc42398531f109931b517e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_zFTL_SSD ;
    dct:source "In MLC (Multi-Level Cell) NAND flash memory, the typical page size is 4KB and each block consists of 128 pages." .

<http://example.org/ssd/assertion/a28d1e18b9f147e7a210275012d3df1e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "Recently, NAND flash memory has become a necessity as a storage medium for mobile consumer electronics devices, thanks to its non-volatility, superior performance, shock resistance, and low-power consumption." .

<http://example.org/ssd/assertion/a77b6060b25f430aa9a050dc13af5e72> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "Our evaluations with five real-world workloads show that zFTL successfully enhances storage performance and lifetime by improving the write amplification factor (WAF) by a factor of 2.6 (geometric mean) compared to the case without compression support." .

<http://example.org/ssd/assertion/c03d94cf891e4806ae94bd7842fd027d> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:reduces ;
    ssd:subject ssd:E1_zFTL_SSD ;
    dct:source "We also find that the proposed IDP is effective in reducing power consumption especially when many of the input data are incompressible." .

<http://example.org/ssd/assertion/d7537b6b3bc8473f9b1fe71d3f3fa143> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_zFTL_SSD ;
    dct:source "Our results show that the use of data compression improves the WAF by a factor of 1.8 to 4.7." .

<http://example.org/ssd/assertion/f05a9b45782540a087f1995385093080> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_zFTL_SSD ;
    dct:source "In this paper, we present a flash translation layer called zFTL, which reduces the amount of data written to NAND flash memory by supporting on-line, transparent data compression based on the X-Match algorithm." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> rdfs:label "Mobile Device" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> .

ssd:E1_zFTL_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "zFTL_SSD" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    ssd:operatesUnder ssd:E2_EC1 ;
    ssd:reduces <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> ;
    rdfs:isDefinedBy tax:SSD .

ssd:E2_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> ;
    ssd:hasWorkloadProfile ssd:E4_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> rdfs:label "Power Consumption",
        "Write Amplification Factor" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance> .

