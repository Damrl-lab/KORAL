<http://example.org/ssd/assertion/1066ba91bd514f60b47c2a05d00eed07> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:degrades ;
    ssd:subject tax:SSD ;
    dct:source "blk-switch achieves similar or higher improvements for P99.9 tail latency." .

<http://example.org/ssd/assertion/139b10e05d574258ad4571f8106b796e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "We use the random read workload in our default setup." .

