    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "L-apps generate 4KB read/write requests with an I/O depth of 1." .

<http://example.org/ssd/assertion/5468d927318b4dfcbd21b6225cfa624e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "L-apps generate 4KB requests and throughput-bound T-apps generate large requests." .

<http://example.org/ssd/assertion/6e67d69f0c074226ad8545bf17d2dab7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "We demonstrate the above using design, implementation and evaluation of blk-switch, a new Linux kernel storage stack architecture." .

<http://example.org/ssd/assertion/92f4f9a1370d4520a28815913438e497> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "L-apps generate 4KB read/write requests with an I/O depth of 1." .

<http://example.org/ssd/assertion/c16328617f0641c1b8b95a4df50eb31b> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "L-apps generate 4KB requests and throughput-bound T-apps generate large requests." .

<http://example.org/ssd/assertion/f9b4a6169c73415db6e6ab60806df105> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject tax:SSD ;
    dct:source "In comparison to Linux, blk-switch improves the average and the P99 latency by as much as 130× and 24×, respectively." .

