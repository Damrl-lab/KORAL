<http://example.org/ssd/assertion/0a3762245ffb41fba8277c9528e9e652> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Each request size ranges from 4 KB to 4 MB." .

<http://example.org/ssd/assertion/0de9a71148be41acb50b7a425883aa46> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Pensieve_SSD ;
    dct:source "Pensieve can reduce the amount of program operations by 19%, while delivering competitive performance." .

<http://example.org/ssd/assertion/56ed8c9987994b47b0ae9a8cd22e5c4e> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Pensieve_SSD ;
    dct:source "Pensieve maintains an average latency of 107 Î¼s, only 13.4% slower than the baseline that does not use any data compression." .

<http://example.org/ssd/assertion/7e562ad73730473cad485c3787960f5d> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "For this workload, we assume 80% of reads and 20% of writes. We also assume 20% of these requests are random, and the rest are sequential." .

<http://example.org/ssd/assertion/95766f8f76174a329b5ece4e0279234f> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Balanced> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
