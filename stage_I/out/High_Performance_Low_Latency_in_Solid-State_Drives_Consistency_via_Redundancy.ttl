<http://example.org/ssd/assertion/4e8a5a5afd9749d4a452f633991e5f55> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "Many systems with mixed workloads have low latency requirements for reads and in such cases SSD inconsistencies become a problem." .

<http://example.org/ssd/assertion/592aa2d5742b4f18b1bbd59a3b7096c2> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:degradedBy ;
    ssd:subject ssd:E1_SSD ;
    dct:source "For example, performing a mixture of large sequential and random writes over a large part of a driveâ€™s logical range can lead to high latencies. This is often due to the garbage collector not being able to keep up leading to what is known as internal fragmentation." .

<http://example.org/ssd/assertion/7c0829fc7e354ece807fbec4cbc0f50c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Our design solves this problem while doubling the read throughput." .

<http://example.org/ssd/assertion/7ef605b6876f494da3f9d6829ed753e4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Solid-state drives are becoming increasingly popular in enterprise storage systems, playing the role of large caches and permanent storage." .

<http://example.org/ssd/assertion/a8a9a8522ce94216a43801192e155b44> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Many systems with mixed workloads have low latency requirements for reads and in such cases SSD inconsistencies become a problem." .

<http://example.org/ssd/assertion/b253bad83a724558b992d4f9f821e96c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_SSD ;
    dct:source "Our approach achieves low read latencies consistently." .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasWorkloadProfile ssd:E3_WorkloadProfile ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:E3_WorkloadProfile a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WorkloadProfile" ;
    ssd:hasReadWriteMix <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> rdfs:label "Read-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> rdfs:label "Garbage Collection" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

ssd:E1_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "SSD" ;
    ssd:degradedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

