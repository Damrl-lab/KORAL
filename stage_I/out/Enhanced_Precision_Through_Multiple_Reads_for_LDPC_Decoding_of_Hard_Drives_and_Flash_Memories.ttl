<http://example.org/ssd/assertion/4767b10defa7442f8038a830ee8c3257> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object tax:SSD ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/SLC> ;
    dct:source "The original NAND Flash uses only two levels. This is called single-level-cell (SLC) Flash." .

<http://example.org/ssd/assertion/694ac94a762e489d911952857bc77a71> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E6_Mutual_Information_Maximization ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E5_Enhanced_Precision ;
    dct:source "The word-line voltages are optimized by maximizing the mutual information (MI) of the quantized channel." .

<http://example.org/ssd/assertion/9bce08f40876437e8ca829b5346bb4a9> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object tax:SSD ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    dct:source "Devices currently available using four levels are called multi-level-cell (MLC) Flash." .

<http://example.org/ssd/assertion/f9aa6ccca0a54859b0c0010eaad3e9ae> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_Enhanced_Precision ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E2_LDPC_Decoding ;
    dct:source "Enhanced precision through multiple reads is crucial to successfully reaping the benefits of LDPC coding in Flash memory." .

ssd:E2_LDPC_Decoding a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529>,
        owl:Thing ;
    rdfs:label "LDPC Decoding" ;
    ssd:improves ssd:E5_Enhanced_Precision ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

ssd:E6_Mutual_Information_Maximization a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529>,
