ssd:E12_CFQ a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler>,
        owl:Thing ;
    rdfs:label "CFQ" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler> .

ssd:E13_FIFO a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler>,
        owl:Thing ;
    rdfs:label "FIFO" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/I/O%2520Scheduler> .

ssd:axiom_7442aee1 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_bda2e0d4 a owl:Axiom ;
    rdfs:comment "hasAccessPattern rdfs:domain WorkloadProfile; rdfs:range AccessPattern." .

ssd:axiom_ea588d8b a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/8b3a39cf334a40ef9fb4f0e554a404e4> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E11_BFQ_ ;
    dct:source "The scheduling decisions made by BFQ+ comply with keeping a high throughput also with flash-based devices." .

<http://example.org/ssd/assertion/8c81bea1c0a34301ad0fe03ca749ef5e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E11_BFQ_ ;
    dct:source "Interactive applications now experience almost the same latency as if the disk was idle." .

<http://example.org/ssd/assertion/add40dfad17941e0ad74b2700e90cc1e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Sequential> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    dct:source "BFQ+ achieves an about 24% higher throughput than CFQ with sequential workloads." .

<http://example.org/ssd/assertion/e14f23f8678243a6868c150a4d5a28f7> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E11_BFQ_ ;
    dct:source "BFQ+ achieves up to 30% higher throughput than CFQ under most workloads." .

<http://example.org/ssd/assertion/e9f4921c704a40f8abd69e25d92fe629> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    dct:source "The performance of BFQ+ with 10r-rand is however comparable to CFQ." .

<http://example.org/ssd/assertion/f2243f874ea342b49b2e19e1d971d7d8> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    dct:source "We have implemented a suite of micro and macro benchmarks mimicking several real-world tasks." .

ssd:degrades a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

