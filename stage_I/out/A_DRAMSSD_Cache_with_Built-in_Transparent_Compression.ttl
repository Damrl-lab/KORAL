<http://example.org/ssd/assertion/1e2a5a6a32654fe8914ea48ebba09871> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:reduces ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "Our experimental results show that ZipCache can achieve up to 72.4% higher throughput and 42.4% lower latency, while reducing the write amplification by up to 26.2 times." .

<http://example.org/ssd/assertion/27a01011999b4d4d8586f00f572cc71b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> ;
    ssd:predicate ssd:hasInterfaceType ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "Host CPU accesses the SSD through standard I/O interface (e.g., NVMe)." .

<http://example.org/ssd/assertion/58492de95eae4f5c8533fe265133f56d> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "The dedicated hardware engines on the controller SoC implement per-4KB (de)compression at the latency of a few microseconds." .

<http://example.org/ssd/assertion/6081ef0d3614490c8750e9f1b2e6eb82> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Read-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject ssd:E3_WorkloadProfile ;
    dct:source "Our experimental results show that ZipCache can achieve up to 72.4% higher throughput and 42.4% lower latency, while reducing the write amplification by up to 26.2 times." .

<http://example.org/ssd/assertion/714609055e334da895d0530585ab6991> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "As a core component in modern data centers, key-value cache provides high-throughput and low-latency services for high-speed data processing." .

<http://example.org/ssd/assertion/861d6433504d49718f913b2f6735cbd5> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "Our experimental results show that ZipCache can achieve up to 72.4% higher throughput and 42.4% lower latency, while reducing the write amplification by up to 26.2 times." .

<http://example.org/ssd/assertion/9d217200a9054438931a222b23338173> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "Our experimental results show that ZipCache can achieve up to 72.4% higher throughput." .

<http://example.org/ssd/assertion/bc7fc7d810734479aff12ac03c2b79b3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "ZipCache can achieve up to 42.4% lower 90-percentile read latency." .

<http://example.org/ssd/assertion/fa4b35841c5c4a3da37ebd72b13aff38> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_ZipCache ;
    dct:source "Our experimental results show that ZipCache can achieve up to 42.4% lower latency." .

ssd:degrades a owl:ObjectProperty .

