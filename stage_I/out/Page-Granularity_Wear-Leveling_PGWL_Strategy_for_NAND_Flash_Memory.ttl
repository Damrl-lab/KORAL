<http://example.org/ssd/assertion/2282d21106f3431596975da8a6635092> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Bad%2520Block%2520Management> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "Experimental results show that PGWL can extend the lifetime of 2 /C2-nm NAND flash memory by 88.3% compared with traditional bad block management (BBM)." .

<http://example.org/ssd/assertion/3eb7a095fdc0439ba3339d76a76e14b9> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Endurance> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "Experimental results show that PGWL can extend the lifetime of 2 /C2-nm NAND flash memory by 88.3% compared with traditional bad block management (BBM)." .

<http://example.org/ssd/assertion/403f59b303f1454a8f5c0bcaf37289e9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "We present a practical average RBER prediction model to evaluate the reliability of flash pages using the system clock of the sink node." .

<http://example.org/ssd/assertion/61d09da525a04c4886c80cd425fd0022> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "NAND flash memory is often employed in sink nodes because of its excellent characteristics." .

<http://example.org/ssd/assertion/66f944909aa8479b9ad1e34a3dbf7ac4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "Wear leveling is a vital technique for improving the effective endurance of NAND flash memory." .

<http://example.org/ssd/assertion/727cdaa9d7a24e8dafe65c89c0d094e1> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "When weak pages are found, they should be relieved to increase the number of P/E cycles that the error correction code (ECC) can tolerate." .

<http://example.org/ssd/assertion/cad1faf408db4ed89662a3396d4557b2> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E3_WorkloadProfile ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "The PGWL strategy enables self-adaptive leveling of the RBER growths of different pages in real time by introducing page-granularity wear leveling instead of block-granularity wear leveling to exploit the lifetime potency of each page in a block." .

<http://example.org/ssd/assertion/d6694a52bd9944578e0239afb93d423a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E4_NAND_Flash_Memory ;
    dct:source "NAND flash memory is often employed in sink nodes because of its excellent characteristics." .

