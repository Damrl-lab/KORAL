<http://example.org/ssd/assertion/69ee09e661a74d59b682ac4869f8a50b> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object ssd:E6_Read_Performance ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E8_LDPC ;
    dct:source "The proposed REAL scheme can reduce the LDPC decoding latency by 26.44% and 33.05%, compared with the Logarithm Domain Min-Sum (LD-MS) and Probability Domain BP (PD-BP) schemes, respectively." .

<http://example.org/ssd/assertion/8108821f9fdf4cd8aea56e6da697c534> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_MLC_SSD_1 ;
    dct:source "NAND flash with the advantages of high performance, large storage capacity, low power consumption, shock resistance, and non-volatility is widely used in various computer systems and consumer electronics products as a storage device." .

<http://example.org/ssd/assertion/f4d1f0486e884d729728898d7ee9651d> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E10_Numerical_Correlation_Characteristic ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject ssd:E9_Retention_Errors ;
    dct:source "It has been observed that flash retention errors show the so-called numerical-correlation characteristic (i.e., the 0-1 bits stored in the flash cell affect each other with the leakage of the charge) in each flash cell." .

ssd:E2_MLC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "MLC_SSD_1" ;
    ssd:operatesUnder ssd:E4_EC1 ;
    rdfs:isDefinedBy tax:SSD .

ssd:E8_LDPC a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529>,
        owl:Thing ;
    rdfs:label "LDPC" ;
    ssd:improves ssd:E6_Read_Performance ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

ssd:E9_Retention_Errors a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529>,
        owl:Thing ;
    rdfs:label "Retention Errors" ;
    ssd:correlatesWith ssd:E10_Numerical_Correlation_Characteristic ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

