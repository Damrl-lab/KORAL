<http://example.org/ssd/assertion/0d6d7839b16d474fb635e46ecac292f9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E7_EnvironmentalAndOperationalContext ;
    dct:source "The reliability of flash memory is not only affected by P/E cycles and data retention time, but also influenced by the temperature and process variations." .

<http://example.org/ssd/assertion/75bc087de4b14587a15ed5e047c80d6b> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_3D_NAND_Flash_Memory ;
    dct:source "The 3D NAND flash memory has a vertically stacked structure, which includes word-lines and bit lines located in different layers." .

<http://example.org/ssd/assertion/8d51671583e24643bb95adaf4e8660cc> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:predicate ssd:hasStructure ;
    ssd:subject ssd:E1_3D_NAND_Flash_Memory ;
    dct:source "Three-dimensional (3D) NAND flash memory was proposed to further increase the storage capacity by vertically stacking multiple layers." .

<http://example.org/ssd/assertion/a81e3c3064504c5fb2ca05629494c8ae> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E7_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_3D_NAND_Flash_Memory ;
    dct:source "The reliability of flash memory is not only affected by P/E cycles and data retention time, but also influenced by the temperature and process variations." .

<http://example.org/ssd/assertion/cc97ce8f777740908b43f265119af9b6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E6_LDPC_Codes ;
    ssd:predicate ssd:uses ;
    ssd:subject ssd:E1_3D_NAND_Flash_Memory ;
    dct:source "Recently, low-density parity-check (LDPC) codes with hard and soft decision decoding have been considered for 3D flash memory." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E6_LDPC_Codes a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529>,
        owl:Thing ;
    rdfs:label "LDPC Codes" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

ssd:E7_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasTemperature <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

<http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> rdfs:label "Error Correction (ECC)" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/DataPath/Error%2520Correction%2520%2528ECC%2529> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> rdfs:label "3D" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> .

ssd:E1_3D_NAND_Flash_Memory a tax:SSD,
        owl:Thing ;
    rdfs:label "3D NAND Flash Memory" ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> ;
    ssd:hasStructure <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/Structure/3D> ;
    ssd:operatesUnder ssd:E7_EnvironmentalAndOperationalContext ;
    ssd:uses ssd:E6_LDPC_Codes ;
    rdfs:isDefinedBy tax:SSD .

