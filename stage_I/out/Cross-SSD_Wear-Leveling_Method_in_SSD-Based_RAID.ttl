ssd:axiom_06678582 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern." .

ssd:axiom_3a67c000 a owl:Axiom ;
    rdfs:comment "ensures rdfs:domain SSD/InternalFirmwareAndLogic/MediaManagement/Wear Leveling; rdfs:range SSD/InternalFirmwareAndLogic/MediaManagement/Wear Leveling." .

ssd:axiom_42d9278d a owl:Axiom ;
    rdfs:comment "uses rdfs:domain SSD; rdfs:range SSD/InternalFirmwareAndLogic/MediaManagement/Wear Leveling." .

ssd:axiom_58de53fb a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range SSD/MetricsHealthAndState/Performance." .

ssd:axiom_8dd9c033 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range SSD/MetricsHealthAndState/Endurance." .

ssd:axiom_8f715dc8 a owl:Axiom ;
    rdfs:comment "isCompatibleWith rdfs:domain SSD; rdfs:range SSD/ConfigurationAndAdvancedParadigms/RAIDAndRedundancy/RAID5." .

