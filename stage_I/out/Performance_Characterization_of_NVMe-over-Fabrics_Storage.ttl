<http://example.org/ssd/assertion/1d4936bd53734a28a6d79af13a778d12> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_NVMe_oF ;
    dct:source "NVMe-oF performance is almost identical to DAS over the entire load range—the average latencies are within a 3.5% difference." .

<http://example.org/ssd/assertion/4cf2112475e4440ab9577956fa2524c2> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_NVMe_oF ;
    ssd:predicate ssd:supports ;
    ssd:subject ssd:E1_NVMe_SSD ;
    dct:source "NVMe-over-Fabrics (NVMe-oF) is a recent protocol standard for accessing NVMe devices over RDMA-capable networks." .

<http://example.org/ssd/assertion/50decc928c934d80a4bdc1fe58f2c071> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_NVMe_oF ;
    dct:source "NVMe-oF tail latencies are slightly better than DAS for light to medium-high loads." .

<http://example.org/ssd/assertion/67303a53b04d47919743048b5f0594ea> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_NVMe_oF ;
    dct:source "A single current-generation NVMe-SSD sustains up to one million IOPS with a 90 μs read latency." .

<http://example.org/ssd/assertion/8c77b4fbf761425cb2b55db950334486> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_NVMe_oF ;
    dct:source "NVMe-oF minimizes the performance degradation experienced by the application, thus significantly improving the efficiency and viability of NVMe disaggregation." .

<http://example.org/ssd/assertion/a0c387552b6d4d5488435e8e2c40d5d9> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E8_iSCSI ;
    dct:source "iSCSI throughput is significantly lower than DAS and NVMe-oF." .

<http://example.org/ssd/assertion/a47b3146b9f54f9f9d8bac4e0a798a02> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E1_NVMe_SSD ;
    ssd:predicate ssd:evaluates ;
    ssd:subject ssd:E9_RocksDB ;
    dct:source "We use RocksDB as examples for real-world I/O intensive workloads, and show that they experience minimal performance degradation when running on remote storage." .

<http://example.org/ssd/assertion/b9d127d3f118440ca641bdfa39a99768> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E1_NVMe_SSD ;
    ssd:predicate ssd:evaluates ;
    ssd:subject ssd:E10_MySQL ;
    dct:source "We use MySQL as examples for real-world I/O intensive workloads, and show that they experience minimal performance degradation when running on remote storage." .

<http://example.org/ssd/assertion/cc0bd2d7dd2f4a94b386bee01766ffb2> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E8_iSCSI ;
    dct:source "iSCSI latencies are noticeably longer compared to local storage access." .

<http://example.org/ssd/assertion/fad7d275c0a44f779b5d5afad3f82856> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_NVMe_SSD ;
    dct:source "NVMe-SDDs (i.e., PCIe-based SSDs) are becoming increasingly popular for serving I/O intensive applications in datacenters and enterprise deployments." .

ssd:E10_MySQL a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/ValidationAndBenchmarks>,
        owl:Thing ;
    rdfs:label "MySQL" ;
    ssd:evaluates ssd:E1_NVMe_SSD ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/ValidationAndBenchmarks> .

ssd:E9_RocksDB a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/ValidationAndBenchmarks>,
        owl:Thing ;
    rdfs:label "RocksDB" ;
    ssd:evaluates ssd:E1_NVMe_SSD ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/ValidationAndBenchmarks> .

ssd:degrades a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E8_iSCSI a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/Interconnects>,
        owl:Thing ;
    rdfs:label "iSCSI" ;
    ssd:degrades <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/Interconnects> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> rdfs:label "99th Percentile Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Average Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

ssd:E1_NVMe_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "NVMe-SSD" ;
    ssd:operatesUnder <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:supports ssd:E3_NVMe_oF ;
    rdfs:isDefinedBy tax:SSD .

ssd:E3_NVMe_oF a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/Interconnects/NVMe-oF%2520%2528RoCE%252C%2520TCP%252C%2520FC%2529>,
        owl:Thing ;
    rdfs:label "NVMe-oF" ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/99th%2520Percentile%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/Interconnects/NVMe-oF%2520%2528RoCE%252C%2520TCP%252C%2520FC%2529> .

