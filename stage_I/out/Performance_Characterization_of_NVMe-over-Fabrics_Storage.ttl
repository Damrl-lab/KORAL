ssd:axiom_5857bb0b a owl:Axiom ;
    rdfs:comment "improves rdfs:domain EcosystemAndIntegrationContext/Interconnects; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_c61ac301 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_f23f1d4c a owl:Axiom ;
    rdfs:comment "supports rdfs:domain SSD; rdfs:range EcosystemAndIntegrationContext/Interconnects." .

