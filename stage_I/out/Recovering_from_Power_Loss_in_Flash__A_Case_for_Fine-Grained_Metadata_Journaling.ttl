ssd:axiom_075ee50b a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_225394c5 a owl:Axiom ;
    rdfs:comment "improves rdfs:domain SSD; rdfs:range MetricsHealthAndState/Performance." .

ssd:axiom_6db9c26d a owl:Axiom ;
    rdfs:comment "hasJournalMode rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic." .

ssd:axiom_a0a1f935 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_c58aa204 a owl:Axiom ;
    rdfs:comment "hasCheckpointing rdfs:domain SSD; rdfs:range InternalFirmwareAndLogic." .

ssd:axiom_ed7c0428 a owl:Axiom ;
    rdfs:comment "hasFileSystem rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext/HostSystem/File System." .

ssd:confidence a owl:DatatypeProperty .

