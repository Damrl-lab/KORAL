ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/2861a43ff5cc4187ad0e7a4e5b4e733d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E7_EnvironmentalAndOperationalContext ;
    dct:source "F2FS, a log-structured filesystem, has gained widespread adoption in Android systems." .

<http://example.org/ssd/assertion/95f8f6775a2b4b61938dce9cc984a200> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_Checkpointing ;
    ssd:predicate ssd:hasCheckpointing ;
    ssd:subject ssd:E2_F2FS ;
    dct:source "F2FS, however, relies on coarse-grained checkpointing for crash recovery, which once triggered, severely degrades the system performance by blocking file reads/writes." .

<http://example.org/ssd/assertion/b51f7abda0b4405c824d4aa18fdc468a> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E4_Ordered_Journal_Mode ;
    ssd:predicate ssd:hasJournalMode ;
    ssd:subject ssd:E1_F2FSJ ;
    dct:source "In this paper, we propose a novel journaling technique, called F2FSJ, for F2FS with ordered journal mode." .

<http://example.org/ssd/assertion/d61594107c9f41d9a4f6be82a9a8b7a3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_F2FSJ ;
    dct:source "F2FSJ is open-sourced for public access." .

<http://example.org/ssd/assertion/f1427ee80a9a4a2f94c3715dfabe295a> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_F2FSJ ;
    dct:source "Our experimental results demonstrate that F2FSJ can effectively reduce the checkpointing time by up to 4.9x and reduce the latency by up to 35% compared with F2FS." .

<http://example.org/ssd/assertion/fb3b9d8204ca4098bcdfaad663a0c237> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:predicate ssd:hasFileSystem ;
    ssd:subject ssd:E1_F2FSJ ;
    dct:source "In this paper, we propose a novel journaling technique, called F2FSJ, for F2FS with ordered journal mode." .

ssd:E2_F2FS a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "F2FS" ;
    ssd:hasCheckpointing ssd:E5_Checkpointing ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_Ordered_Journal_Mode a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "Ordered Journal Mode" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

ssd:E5_Checkpointing a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "Checkpointing" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

ssd:E7_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> rdfs:label "Mobile Device" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> rdfs:label "File System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> rdfs:label "Latency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> .

ssd:E1_F2FSJ a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic>,
        owl:Thing ;
    rdfs:label "F2FSJ" ;
    ssd:hasFileSystem <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> ;
    ssd:hasJournalMode ssd:E4_Ordered_Journal_Mode ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Average%2520Latency> ;
    ssd:operatesUnder ssd:E7_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic> .

