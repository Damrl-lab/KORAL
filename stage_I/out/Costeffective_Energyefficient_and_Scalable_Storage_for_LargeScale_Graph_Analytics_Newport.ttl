ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/030ed67a999e44a4b427f63ddbb68500> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ComputationalStorage> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E1_Newport ;
    dct:source "Newport, a high-performance and energy-efficient computational storage developed for realizing the full potential of in-storage processing." .

<http://example.org/ssd/assertion/1c044ef229234b7397918a295a18860c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_Newport ;
    dct:source "Newport is the first commodity SSD that can be configured to run a server-like operating system." .

<http://example.org/ssd/assertion/27ce373b3a104e99827a61892d91a40b> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Newport ;
    dct:source "The results demonstrate that data-intensive AI workloads can be efficiently parallelized and offloaded, even to a small set of Newport drives with significant performance gains and energy savings." .

<http://example.org/ssd/assertion/3f4d0909518242b8a8b1b54b303ff2f3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E8_ARM_Cortex_A53 ;
    ssd:predicate ssd:hasController ;
    ssd:subject ssd:E1_Newport ;
    dct:source "Newport is equipped with general-purpose, multi-core processors and multiple GBs of DRAM." .

<http://example.org/ssd/assertion/74250cdc56094817bfe9185018b21e2d> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Newport ;
    dct:source "The results demonstrate that data-intensive AI workloads can be efficiently parallelized and offloaded, even to a small set of Newport drives with significant performance gains." .

<http://example.org/ssd/assertion/7c3c94781ff24f20a02b99314c881755> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "Newport is an enterprise-grade solid state drive." .

<http://example.org/ssd/assertion/b1712318cb9d46389a75f19cea10216f> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_Newport ;
    dct:source "The results demonstrate that data-intensive AI workloads can be efficiently parallelized and offloaded, even to a small set of Newport drives with significant performance gains." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E8_ARM_Cortex_A53 a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller/Cores>,
        owl:Thing ;
    rdfs:label "ARM Cortex-A53" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/Controller/Cores> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ComputationalStorage> rdfs:label "Computational Storage" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ComputationalStorage> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> rdfs:label "Enterprise Server" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState> rdfs:label "Energy Efficiency" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:E1_Newport a tax:SSD,
        owl:Thing ;
    rdfs:label "Newport" ;
    ssd:hasController ssd:E8_ARM_Cortex_A53 ;
    ssd:hasDeviceType <http://example.org/ssd/taxonomy/SSD/ConfigurationAndAdvancedParadigms/ComputationalStorage> ;
    ssd:improves <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS>,
        <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

