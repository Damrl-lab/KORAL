@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_0302c056 a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_255f3fe0 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:confidence a owl:DatatypeProperty .

ssd:correlatesWith a owl:ObjectProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/12a85916ff9b4d439870154007bdd9c1> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "The detrimental effects of random writes could be reduced by the log-structured file system (LFS) approach." .

<http://example.org/ssd/assertion/145594bf9fbf4af08b20c96c3776c660> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E5_BTRFS ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "F2FS shows significantly better performance than other file systems and outperforms EXT4 by up to 2×." .

<http://example.org/ssd/assertion/19e13ea0ab5649eeb5183d85c4e9d0de> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "The oltp workload generates a large number of random writes and fsync calls on a single 800MB database file." .

<http://example.org/ssd/assertion/318a4e1a3a574f0695cf25bfdea50802> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E4_EXT4 ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "F2FS reduces the elapsed time by 20% (Facebook-app) and 40% (Twitter-app) compared to EXT4." .

<http://example.org/ssd/assertion/3e1840a1c1b34d33af9c4c44d0630970> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_Server_System ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "On a server system, F2FS is shown to perform better than EXT4 by up to 2.5× (SATA SSD) and 1.8× (PCIe SSD)." .

<http://example.org/ssd/assertion/68150be1fffe42f58a1b6dc5734dbe21> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "F2FS turns over 90% of 4KB random writes into 512KB sequential writes." .

<http://example.org/ssd/assertion/7262f463525d4ea8bf4bac024641c05f> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_Server_System ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E10_SATA_SSD ;
    dct:source "In the case of the server system, we harness a SATA SSD and a (higher-speed) PCIe SSD." .

<http://example.org/ssd/assertion/9630333db9fd40699cda9bb12df71c3e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E3_Server_System ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E11_PCIe_SSD ;
    dct:source "In the case of the server system, we harness a SATA SSD and a (higher-speed) PCIe SSD." .

<http://example.org/ssd/assertion/a4854c460c80496384105804e71e84fe> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_Mobile_System ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "Experimental results highlight the desirable performance of F2FS; on a state-of-the-art mobile system, it outperforms EXT4 under synthetic workloads by up to 3.1× (iozone) and 2× (SQLite)." .

<http://example.org/ssd/assertion/b3d50aa178034652990c147d892abdaf> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object ssd:E6_NILFS2 ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E1_F2FS ;
    dct:source "NILFS2 achieves similar performance with a nearly identical amount of data writes compared to EXT4." .

ssd:E10_SATA_SSD a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA>,
        owl:Thing ;
    rdfs:label "SATA SSD" ;
    ssd:operatesUnder ssd:E3_Server_System ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/SATA> .

ssd:E11_PCIe_SSD a <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe>,
        owl:Thing ;
    rdfs:label "PCIe SSD" ;
    ssd:operatesUnder ssd:E3_Server_System ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/InterfaceType/NVMe> .

ssd:E2_Mobile_System a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device>,
        owl:Thing ;
    rdfs:label "Mobile System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Mobile%2520Device> .

ssd:E4_EXT4 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "EXT4" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:E5_BTRFS a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "BTRFS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:E6_NILFS2 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "NILFS2" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> rdfs:label "Write-Heavy" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E3_Server_System a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server>,
        owl:Thing ;
    rdfs:label "Server System" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> .

ssd:E1_F2FS a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System>,
        owl:Thing ;
    rdfs:label "F2FS" ;
    ssd:hasWorkloadProfile <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern/Random>,
        <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize/4KB>,
        <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:improves ssd:E4_EXT4,
        ssd:E5_BTRFS,
        ssd:E6_NILFS2 ;
    ssd:operatesUnder ssd:E2_Mobile_System,
        ssd:E3_Server_System ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/HostSystem/File%2520System> .

