<http://example.org/ssd/assertion/1a45039341684a3b814675637458d770> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E10_MQFQ ;
    dct:source "Our implementation of MQFQ in Linux 4.15 demonstrates both fairness and high throughput." .

<http://example.org/ssd/assertion/4c05ab687200481a918a1c34513ab541> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    dct:source "The FIO queue depth (i.e., the number of submitted but not yet completed requests) is set to 128—large enough to maintain maximum throughput to the device." .

<http://example.org/ssd/assertion/6a0309bea05f479e8c13322feabca6e7> a ssd:Assertion ;
    ssd:confidence 0.7 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasAccessPattern ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    dct:source "Evaluation with an NVMe over RDMA fabric (NVMf) device shows that MQFQ can reach up to 3.1 Million IOP/s on a single machine." .

<http://example.org/ssd/assertion/76ffdabe214e471caf7b94f5b32ce865> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E11_FlashX ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    dct:source "MQFQ reduces the slowdown caused by an antagonist from 3:78×to1:33×for the FlashX workload." .

<http://example.org/ssd/assertion/7837efa131774be587a5320f77eb5383> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "Evaluation with an NVMe over RDMA fabric (NVMf) device shows that MQFQ can reach up to 3.1 Million IOP/s on a single machine." .

<http://example.org/ssd/assertion/a2000f6f4b1f47ce9e9e719c68c4f9fd> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    dct:source "Evaluation with an NVMe over RDMA fabric (NVMf) device shows that MQFQ can reach up to 3.1 Million IOP/s on a single machine." .

<http://example.org/ssd/assertion/e258545518cb43079af83784cf69cef5> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E10_MQFQ ;
    dct:source "Evaluation with an NVMe over RDMA fabric (NVMf) device shows that MQFQ can reach up to 3.1 Million IOP/s on a single machine—20×higher than the state-of-the-art Linux Budget Fair Queueing." .

<http://example.org/ssd/assertion/ed6a33ad89c6490d843e4e3481b15e82> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E12_Aerospike ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    dct:source "MQFQ reduces the slowdown caused by an antagonist from 6:57×to1:03×for the Aerospike workload." .

