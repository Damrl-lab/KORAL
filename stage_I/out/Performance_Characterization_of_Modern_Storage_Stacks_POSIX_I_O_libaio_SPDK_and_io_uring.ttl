<http://example.org/ssd/assertion/699e8f8f3f0340a5a2c955189a0320b1> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E9_SPDK ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E8_io_uring ;
    dct:source "At high-loads (128 queue depth, 7 devices), io_uring is more than an order of magnitude inefficient than SPDK." .

<http://example.org/ssd/assertion/7bef7d8c9a694f068bf82a7084a069d1> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:predicate ssd:hasPerformance ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "Our test bed consists of Intel Xeon CPUs with seven (7) Intel Optane devices in a single machine with the peak performance of 4.2 million IOPS." .

<http://example.org/ssd/assertion/9f6d9647570e440ba8c4b797f94008f6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:predicate ssd:hasQueueDepth ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We report on the maximum IOPS completed with 4KiB random reads with a single outstanding request (i.e., queue depth QD=1)." .

<http://example.org/ssd/assertion/d7a5292377bf416db3696ae618d2b32d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:predicate ssd:hasBlockSize ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We report on the maximum IOPS completed with 4KiB random reads with a single outstanding request (i.e., queue depth QD=1)." .

<http://example.org/ssd/assertion/dd34f0f82ee449eb8c831acc185449ff> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E1_Intel_Optane_SSD ;
    dct:source "Our test bed consists of Intel Xeon CPUs with seven (7) Intel Optane devices in a single machine with the peak performance of 4.2 million IOPS." .

<http://example.org/ssd/assertion/f6ab123a2b12458386fbdb964df56ea4> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E10_FIO ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E9_SPDK ;
    dct:source "SPDK can saturate our hardware with 5 cores (using fio) or just a single core when using SPDKâ€™s light-weight perf benchmark." .

<http://example.org/ssd/assertion/fdf19d4f8b5b4c0bbfbaa6398a105bb9> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E2_EnvironmentalAndOperationalContext ;
    dct:source "We report on the maximum IOPS completed with 4KiB random reads with a single outstanding request (i.e., queue depth QD=1)." .

ssd:E8_io_uring a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/HostIOStack>,
        owl:Thing ;
    rdfs:label "io_uring" ;
    ssd:degrades ssd:E9_SPDK ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/HostIOStack> .

ssd:degrades a owl:ObjectProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> rdfs:label "Throughput" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/Throughput> .

ssd:E10_FIO a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/ValidationAndBenchmarks>,
        owl:Thing ;
    rdfs:label "FIO" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/ValidationAndBenchmarks> .

ssd:E1_Intel_Optane_SSD a tax:SSD,
        owl:Thing ;
    rdfs:label "Intel Optane SSD" ;
    ssd:hasPerformance <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> ;
    ssd:operatesUnder ssd:E2_EnvironmentalAndOperationalContext ;
    rdfs:isDefinedBy tax:SSD .

ssd:E9_SPDK a <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/HostIOStack>,
        owl:Thing ;
    rdfs:label "SPDK" ;
    ssd:improves ssd:E10_FIO ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EcosystemAndIntegrationContext/HostIOStack> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> rdfs:label "Random" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> rdfs:label "4KB" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> rdfs:label "QD1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> rdfs:label "IOPS" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Performance/IOPS> .

ssd:E2_EnvironmentalAndOperationalContext a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EnvironmentalAndOperationalContext" ;
    ssd:hasBlockSize <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/BlockSize> ;
    ssd:hasQueueDepth <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/QueueDepth> ;
    ssd:hasWorkloadProfile <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/AccessPattern> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

