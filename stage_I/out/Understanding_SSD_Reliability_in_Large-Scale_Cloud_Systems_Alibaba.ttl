@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ssd: <http://example.org/ssd#> .
@prefix tax: <http://example.org/ssd/taxonomy/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ssd:axiom_42907ad4 a owl:Axiom ;
    rdfs:comment "hasDeploymentEnvironment rdfs:domain EnvironmentalAndOperationalContext; rdfs:range DeploymentEnvironment." .

ssd:axiom_6b433b7f a owl:Axiom ;
    rdfs:comment "hasWorkloadProfile rdfs:domain EnvironmentalAndOperationalContext; rdfs:range WorkloadProfile." .

ssd:axiom_97c8c010 a owl:Axiom ;
    rdfs:comment "operatesUnder rdfs:domain SSD; rdfs:range EnvironmentalAndOperationalContext." .

ssd:axiom_e91e365e a owl:Axiom ;
    rdfs:comment "correlatesWith rdfs:domain SMARTAttribute; rdfs:range Reliability." .

ssd:confidence a owl:DatatypeProperty .

ssd:degradedBy a owl:DatatypeProperty .

ssd:degrades a owl:ObjectProperty .

ssd:hasAccessPattern a owl:DatatypeProperty .

ssd:hasBlockSize a owl:DatatypeProperty .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasEnvironment a owl:ObjectProperty .

ssd:hasExternalFactor a owl:ObjectProperty .

ssd:hasQueueDepth a owl:DatatypeProperty .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasRelativeHumidity a owl:DatatypeProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:impacts a owl:ObjectProperty .

ssd:improvedBy a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:object a owl:ObjectProperty .

ssd:percentile a owl:DatatypeProperty .

ssd:predicate a owl:ObjectProperty .

ssd:subject a owl:ObjectProperty .

ssd:unit a owl:DatatypeProperty .

<http://example.org/ssd/assertion/1c95404417ea412ba099912da15fb423> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E6_WP1 ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject ssd:E5_EC1 ;
    dct:source "Certain cloud services may cause unbalanced usage of SSDs." .

<http://example.org/ssd/assertion/203d915b5efb470889cb59bff0acf4c3> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject ssd:E5_EC1 ;
    dct:source "In this paper, we take a holistic view to examine the reliability of SSD-based storage systems in Alibaba's datacenters." .

<http://example.org/ssd/assertion/2b628ce4eb0942c68cebcaea40d73d78> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object ssd:E4_OS_Level_Error_Events ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/UltraDMA%2520CRC%2520Error%2520Count%2520%2520%255BID%253A%2520199%255D> ;
    dct:source "SSDs heavily affected by UCRC (UltraDMA CRC) errors are 2.7x more likely to lead to OS-level error events." .

<http://example.org/ssd/assertion/8c4dfa5b40d44409a3a0f305a01a79c9> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E5_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject tax:SSD ;
    dct:source "In this paper, we take a holistic view to examine the reliability of SSD-based storage systems in Alibaba's datacenters." .

ssd:correlatesWith a owl:ObjectProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E4_OS_Level_Error_Events a <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability>,
        owl:Thing ;
    rdfs:label "OS-Level Error Events" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> .

ssd:E6_WP1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile>,
        owl:Thing ;
    rdfs:label "WP1" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> .

tax:SSD rdfs:label "SSD" ;
    ssd:operatesUnder ssd:E5_EC1 ;
    rdfs:isDefinedBy tax:SSD .

<http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/UltraDMA%2520CRC%2520Error%2520Count%2520%2520%255BID%253A%2520199%255D> rdfs:label "UltraDMA CRC Error Count" ;
    ssd:correlatesWith ssd:E4_OS_Level_Error_Events ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute/UltraDMA%2520CRC%2520Error%2520Count%2520%2520%255BID%253A%2520199%255D> .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> rdfs:label "Datacenter" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> .

ssd:E5_EC1 a <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext>,
        owl:Thing ;
    rdfs:label "EC1" ;
    ssd:hasDeploymentEnvironment <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Datacenter> ;
    ssd:hasWorkloadProfile ssd:E6_WP1 ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> .

