<http://example.org/ssd/assertion/3736a19663f148039482b57911bacc51> a ssd:Assertion ;
    ssd:confidence 0.85 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "The decreasing raw reliability of NAND flash memory chips has drastically impacted the lifetime of commercial SSDs." .

<http://example.org/ssd/assertion/3f7df7b3352d462387573de53ee3f2b2> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:objectLiteral "25" ;
    ssd:predicate ssd:hasTemperature ;
    ssd:subject ssd:E2_EC1 ;
    dct:source "The operating temperature of DRAM should not exceed 85Â°C." .

<http://example.org/ssd/assertion/616f1e51bc224a9dbdea64df9f2ec79e> a ssd:Assertion ;
    ssd:confidence 0.95 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/TLC> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "We provide rigorous experimental data from state-of-the-art MLC and TLC NAND flash devices on various types of flash memory errors." .

<http://example.org/ssd/assertion/d430a0dd3ec6408fa55b9af3733c56f4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    ssd:predicate ssd:degradedBy ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "Retention errors are caused by charge leakage over time after a flash cell is programmed, and are the dominant source of flash memory errors." .

<http://example.org/ssd/assertion/df99e367e725482c985a2cb8f7d01b68> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E2_EC1 ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "NAND flash memory is ubiquitous in everyday life today because its capacity has continuously increased and cost has continuously decreased over decades." .

<http://example.org/ssd/assertion/f775af2f9e634b849ae4eba6b339c9b5> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> ;
    ssd:predicate ssd:degradedBy ;
    ssd:subject ssd:E3_TLC_SSD_1 ;
    dct:source "Read disturb is a phenomenon in NAND flash memory where reading data from a flash cell can cause the threshold voltages of other (unread) cells in the same block to shift to a higher value." .

ssd:hasDeviceType a owl:ObjectProperty .

ssd:hasTemperature a owl:DatatypeProperty .

ssd:improves a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

<http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> rdfs:label "Temperature" ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/ExternalFactors/Temperature> .

<http://example.org/ssd/taxonomy/SSD/PhysicalHardwareAndArchitecture/FlashTechnology/CellType/MLC> rdfs:label "MLC" ;
