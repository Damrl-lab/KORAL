<http://example.org/ssd/assertion/3a9b0e2949e4468fb2fd7a9088ff2ca4> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E7_PER_WL ;
    ssd:predicate ssd:usesWearLevelingScheme ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "This paper proposes PER-WL, an efficient PER-based wear leveling scheme." .

<http://example.org/ssd/assertion/456ea019d125433bbfdf0825161cac02> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    ssd:predicate ssd:usesMetric ;
    ssd:subject ssd:E7_PER_WL ;
    dct:source "PER-WL explicitly uses PER information of blocks as equalization target." .

<http://example.org/ssd/assertion/46a4789a31f045c8909b6e0740aa174c> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E2_TLC_SSD_1 ;
    dct:source "NAND Flash memory has been widely used as storage devices from embedded systems to laptops, desktops, and datacenters." .

<http://example.org/ssd/assertion/5d8bf45673bc482c8d6033c141f212ee> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/DeploymentEnvironment/Enterprise%2520Server> ;
    ssd:predicate ssd:hasDeploymentEnvironment ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    dct:source "NAND Flash memory has been widely used as storage devices from embedded systems to laptops, desktops, and datacenters." .

<http://example.org/ssd/assertion/b51cd82f75f145bfa84a750366ffe919> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    ssd:predicate ssd:hasWorkloadProfile ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    dct:source "We choose four realistic enterprise workloads." .

<http://example.org/ssd/assertion/b99b1fc1f4274e9ebe2d3023c5b55196> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile/ReadWriteMix/Write-Heavy> ;
    ssd:predicate ssd:hasReadWriteMix ;
    ssd:subject <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext/WorkloadProfile> ;
    dct:source "Since Financial1, MSR-hm0 and MSR-proj0 workloads are write intensive, the effect of PER-based wear leveling is evident." .

ssd:hasReadWriteMix a owl:DatatypeProperty .

ssd:hasWorkloadProfile a owl:ObjectProperty .

ssd:operatesUnder a owl:ObjectProperty .

ssd:E2_TLC_SSD_1 a tax:SSD,
        owl:Thing ;
    rdfs:label "TLC_SSD_1" ;
    ssd:operatesUnder <http://example.org/ssd/taxonomy/SSD/EnvironmentalAndOperationalContext> ;
    ssd:usesWearLevelingScheme ssd:E7_PER_WL ;
    rdfs:isDefinedBy tax:SSD .

ssd:E7_PER_WL a <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling>,
        owl:Thing ;
    rdfs:label "PER-WL" ;
    ssd:usesMetric <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/Reliability> ;
    rdfs:isDefinedBy <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Wear%2520Leveling> .

tax:SSD rdfs:label "SSD" ;
    rdfs:isDefinedBy tax:SSD .

