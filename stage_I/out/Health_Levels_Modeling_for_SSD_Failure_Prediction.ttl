<http://example.org/ssd/assertion/0212e197c0bd427d9d0d6ffc588f3815> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/MetricsHealthAndState/SMARTAttribute> ;
    ssd:predicate ssd:hasDeviceType ;
    ssd:subject ssd:E7_MB1 ;
    dct:source "Although it can change for each manufacturer, commonly gathered data generally includes Raw Read Error Rate, Power On Hours, Power Cycles, Device Temperature, Total LBAs Written, and Total LBAs Read." .

<http://example.org/ssd/assertion/4347e13164704e5ba663a5862b703256> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E2_EnvironmentalAndOperationalContext ;
    ssd:predicate ssd:operatesUnder ;
    ssd:subject ssd:E7_MB1 ;
    dct:source "All six drive models are SATA SSDs." .

<http://example.org/ssd/assertion/5acf8a860e0a4a1fb20235c6649c7221> a ssd:Assertion ;
