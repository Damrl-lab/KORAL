<http://example.org/ssd/assertion/11d17e52bc0d4adbb0914b1ee9ae7dad> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E11_Read_Performance ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E4_XFS ;
    dct:source "XFS and ZFS achieve a scan throughput of only 19–40MiB/s." .

<http://example.org/ssd/assertion/4e94102217924fffafbe4f10516849f0> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject tax:SSD ;
    dct:source "Even on SSDs, which do not perform mechanical seeks, a decline in logical block locality can harm performance." .

<http://example.org/ssd/assertion/73f0b55764a24bcf8d9dea9b0f814c4d> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E11_Read_Performance ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E5_ZFS ;
    dct:source "ZFS over 60 minutes; BetrFS, on the other hand, took less than a minute." .

<http://example.org/ssd/assertion/9cd85c7332f749f2b919449bc320f1f6> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E11_Read_Performance ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E2_ext4 ;
    dct:source "For example, ext4 shows over a 2× slowdown after 1200 git pulls." .

<http://example.org/ssd/assertion/9ce0ba01cf0648d785bd94b03e5e315e> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E11_Read_Performance ;
    ssd:predicate ssd:improves ;
    ssd:subject ssd:E7_BetrFS ;
    dct:source "BetrFS typically outperforms the other file systems in our benchmarks." .

<http://example.org/ssd/assertion/a95cc702c2d44b35925c8a3d69e2def1> a ssd:Assertion ;
    ssd:confidence 0.9 ;
    ssd:object ssd:E11_Read_Performance ;
    ssd:predicate ssd:degrades ;
    ssd:subject ssd:E6_F2FS ;
    dct:source "F2FS took over 50 minutes and ZFS over 60 minutes." .

<http://example.org/ssd/assertion/ada3c2e3c4ee41a7b9443cde7ec14348> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object <http://example.org/ssd/taxonomy/SSD/InternalFirmwareAndLogic/MediaManagement/Garbage%2520Collection> ;
    ssd:predicate ssd:correlatesWith ;
    ssd:subject ssd:E9_HDD ;
    dct:source "Reading these files requires additional seeks, and on hard drives, a few seeks can have an outsized effect on performance." .

<http://example.org/ssd/assertion/bef83cdb99e544c09d8b4a45dbb83dec> a ssd:Assertion ;
    ssd:confidence 0.8 ;
    ssd:object ssd:E11_Read_Performance ;
    ssd:predicate ssd:impacts ;
    ssd:subject ssd:E13_Aging ;
    dct:source "We argue that these slowdowns are caused by poor layout." .

<http://example.org/ssd/assertion/f705a64fbac44239abb8c3fab0ff6809> a ssd:Assertion ;
